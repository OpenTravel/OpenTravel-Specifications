<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.opentravel.org/OTA" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.opentravel.org/OTA" elementFormDefault="qualified">
	<xs:include schemaLocation="UniqueId.xsd"/>
	<xs:include schemaLocation="Privacy.xsd"/>
	<xs:include schemaLocation="PersonName.xsd"/>
	<xs:include schemaLocation="Address.xsd"/>
	<xs:include schemaLocation="Telephone.xsd"/>
	<xs:include schemaLocation="CompanyName.xsd"/>
	<xs:include schemaLocation="TPA_Extensions.xsd"/>
	<xs:element name="Preferences">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Needs of the traveler related to travel experiences.</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element ref="PrefCollection" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="Privacy" type="Privacy"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PrefCollection">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Unique aggregation of customer travel needs.</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element ref="CommonPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="VehicleRentalPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirlinesPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="HotelPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="OtherSrvcPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TPA_Extensions" minOccurs="0"/>
				<xs:element name="Privacy" type="Privacy"/>
			</xs:sequence>
			<xs:attribute name="TravelType" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">Identifes the category of travel for a group of preferences.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CommonPref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Travel needs associated with a collection but independent of specific travel services.</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element ref="NamePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PhonePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AddressPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PaymentFormPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="InterestPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="InsurancePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SeatingPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TicketDistribPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MediaEntertainPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PetInfoPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MealPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="LoyaltyPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SpecRequestPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="RelatedTravelerPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TPA_Extensions" minOccurs="0"/>
				<xs:element name="Privacy" type="Privacy"/>
			</xs:sequence>
			<xs:attribute name="SmokingInd">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation xml:lang="en">Indicates preference of smoking or no-smoking.</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="PrimaryLanguage" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">Identifes the primary language preference for the form of travel represented in a collection.
					 The human language is identified by ISO 639 codes.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AltLanguage" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">Identifes the alternate language preferred by the customer for the form of travel specified in a collection.
					 The human language is identified by ISO 639 codes.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="VehicleRentalPref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Vehicle Rental Preferences.</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element ref="LoyaltyPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="VendorPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PaymentFormPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="VehicleCoveragePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SpecRequestPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="VehicleTypePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="VehicleClassPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirConditionPref" minOccurs="0"/>
				<xs:element ref="TransmissionPref" minOccurs="0"/>
				<xs:element ref="CellPhonePref" minOccurs="0"/>
				<xs:element ref="GlobalNavPref" minOccurs="0"/>
				<xs:element ref="PhysChallEquipPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ChildSeatPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TPA_Extensions" minOccurs="0"/>
				<xs:element name="Privacy" type="Privacy"/>
			</xs:sequence>
			<xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en"> value="Preferred" Indicates the level of preference for the vehicle rental collection.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SmokingInd">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation xml:lang="en">Indicates preference of smoking or no-smoking.</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="CarRenterType" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">Category of renter.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="GasPrePayInd">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation xml:lang="en">Indicates preference for prepaid fuel option.</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirlinesPref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Identifes a collection of preferences for airline travel.</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element ref="LoyaltyPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="VendorPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PaymentFormPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirportOriginPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirportRoutePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirFareRestrictPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="FlightTypePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirEquipPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirCabinPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirSrvcClassPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirSeatPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TicketDistribPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MediaEntertainPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PetInfoPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MealPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SpecRequestPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirSSR_Pref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TPA_Extensions" minOccurs="0"/>
				<xs:element name="Privacy" type="Privacy"/>
			</xs:sequence>
			<xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en"> value="Preferred" Indicates level of preference for the special request.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SmokingInd">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation xml:lang="en">Indicates preference of smoking or no-smoking.</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="AirPassengerType" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">Category of airline passenger, using standard ATPCO codes.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AirTicketType">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation xml:lang="en">Type of airline ticket preferred for this collection.</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Electronic"/>
						<xs:enumeration value="Paper"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="HotelPref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Hotel Preferences used for this collection.</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element ref="LoyaltyPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PaymentFormPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="HotelChainPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PropertyNamePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PropertyLocationPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PropertyTypePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PropertyClassPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PropertyAmenityPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="RoomAmenityPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="RoomLocationPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="BedTypePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="FoodSrvcPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MealPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MediaEntertainPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PetInfoPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="RecreationSrvcPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="BusinessSrvcPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PersonalSrvcPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SecurityFeaturePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PhysChallFeaturePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SpecRequestPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TPA_Extensions" minOccurs="0"/>
				<xs:element name="Privacy" type="Privacy"/>
			</xs:sequence>
			<xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en"> value="Preferred" Indicates the level of preference for the hotel collection.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RatePlanCode" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">Code identifying preferred hotel rate plan for this travel situation</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SmokingInd">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation xml:lang="en">Indicates preference of smoking or no-smoking.</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="HotelGuestType" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">Category of hotel guest.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="OtherSrvcPref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Other travel service preferences.</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element ref="OtherSrvcName" minOccurs="0"/>
				<xs:element ref="VendorPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="LoyaltyPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PaymentFormPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SpecRequestPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TPA_Extensions" minOccurs="0"/>
				<xs:element name="Privacy" type="Privacy"/>
			</xs:sequence>
			<xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en"> value="Preferred" Indicates the level of preference for other travel services.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OtherServiceType" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">Category of other travel services.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="NamePref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">PersonName to be used with this collection of preferences.</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element ref="UniqueId"/>
				<xs:element name="PersonName" type="PersonName"/>
			</xs:sequence>
			<xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en"> value="Preferred" Indicated level of preference for the name indicated.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="PhonePref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Telephone number(s) to be used with this collection of preferences.</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element name="Telephone" type="Telephone"/>
			</xs:sequence>
			<xs:attribute name="PhoneUseType" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">Type of telephone number to be used for this preference collection.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AddressPref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Address(es) to be used with this collection of preferences.</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element name="Address" type="Address"/>
			</xs:sequence>
			<xs:attribute name="AddressType">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation xml:lang="en">Defines what the purpose of the address is, such as home, business, mailing, delivery, shipping/receiving, etc.
						</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Home"/>
						<xs:enumeration value="Business"/>
						<xs:enumeration value="Delivery"/>
						<xs:enumeration value="Mailing"/>
						<xs:enumeration value="Billing"/>
						<xs:enumeration value="CreditCard"/>
						<xs:enumeration value="Other"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="PaymentFormPref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Form(s) of payment to be used with this collection of preferences.</xs:documentation>
			</xs:annotation>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en"> value="Preferred" Indicates the level of preference for the payment form to be used with the indicated preference.
								</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="RPH" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="en"> Identifies a specific form of payment by index number to be used to reference the payment form used in this travel collection.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="InterestPref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Traveler interests to be used with this collection of preferences.</xs:documentation>
			</xs:annotation>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en"> value="Preferred" Indicated level of preference for the interest indicated.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="InsurancePref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Identifies travel insurance policies to be used with this collection of preferences.</xs:documentation>
			</xs:annotation>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en"> value="Preferred" Indicates level of preference for the insurance indicated.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SeatingPref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Seating preferences to be used with this collection of preferences.</xs:documentation>
			</xs:annotation>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en"> value="Preferred" Indicated level of preference for the seating indicated.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="SeatDirection" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="en">Direction seat faces during travel, e.g.: forward, backward, etc., when conveyance allows.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="SeatLocation" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="en">Location of seat in cabin of conveyance. Suggested values include: Forward, Middle, Aft, ExitRow, Bulkhead, Right or Left Side, etc.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="SeatPosition" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="en">Preferred position of seat in a row, such as Aisle, Middle, Center, Window, etc.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="SeatRow" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="en">Preferred row for seating; indicates specific row number and/or seat identifier.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="TicketDistribPref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Type of ticket distribution to be used with this collection of preferences.</xs:documentation>
			</xs:annotation>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en"> value="Preferred" Indicated level of preference for the type of ticket distribution.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="DistribType" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="en">Ticket distribution method; such as Fax, Email, Courier, Mail, Airport_Pickup, City_Office, Hotel_Desk,
							 WillCall, etc.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="TicketTime" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="en">Ticket turnaround time desired, amount of time requested to deliver tickets.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="MediaEntertainPref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Media and entertainment information preferences.</xs:documentation>
			</xs:annotation>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en"> value="Preferred" Identifies the customer's level of preference for the media and entertainment identified.
								</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="PetInfoPref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Indicates the preferences for information about pets that accompany the customer in a given travel situation.
				</xs:documentation>
			</xs:annotation>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en"> value="Preferred" Indicated level of preference for the pet information identifiied.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="MealPref">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation xml:lang="en">Food and beverage preferences to be used with this collection.</xs:documentation>
			</xs:annotation>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en"> value="Preferred" Indicates the level of preference for the type of food and beverage indicated.</xs:documentation>
