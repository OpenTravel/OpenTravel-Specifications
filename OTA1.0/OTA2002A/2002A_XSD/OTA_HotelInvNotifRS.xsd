<?xml version="1.0" encoding="UTF-8"?><xs:schema targetNamespace="http://www.opentravel.org/OTA/2002/08" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.opentravel.org/OTA/2002/08" elementFormDefault="qualified" version="2002A">	<xs:include schemaLocation="OTA_HotelCommonTypes.xsd"/>	<xs:element name="OTA_HotelInvNotifRS">		<xs:annotation>			<xs:documentation xml:lang="en">The Hotel Inventory Notification Response message returns the status of processing an Inventory Notification message, and optionally returns a cross-reference of the inventory codes received.</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:choice>				<xs:sequence>					<xs:element name="Success" type="SuccessType"/>					<xs:element name="Warnings" type="WarningsType" minOccurs="0"/>					<xs:element name="InventoryCrossRef" type="InventoryCrossRefInvNotifType" minOccurs="0" maxOccurs="unbounded"/>				</xs:sequence>				<xs:element name="Errors" type="ErrorsType"/>			</xs:choice>			<xs:attributeGroup ref="OTA_PayloadStdAttributes"/>		</xs:complexType>	</xs:element>	<xs:complexType name="InventoryCrossRefInvNotifType">		<xs:attribute name="RequestInvCode" type="xs:string">			<xs:annotation>				<xs:documentation xml:lang="en">Identification of the inventory. The Inventory Code assigned by the sending system for the inventory item upon creation of a new inventory item.</xs:documentation>			</xs:annotation>		</xs:attribute>		<xs:attribute name="RequestInvGroupingCode" type="xs:string">			<xs:annotation>				<xs:documentation xml:lang="en">Identification of the inventory group. The InventoryGrouping  Code assigned by the sending system for the inventory item upon creation of a new inventory grouping.</xs:documentation>			</xs:annotation>		</xs:attribute>		<xs:attribute name="ResponseInvCode" type="xs:string">			<xs:annotation>				<xs:documentation xml:lang="en">Identification of the inventory. The Inventory Code assigned by the receiving system for the inventory item in response to a notification of a new inventory item</xs:documentation>			</xs:annotation>		</xs:attribute>		<xs:attribute name="ResponseInvGroupingCode" type="xs:string">			<xs:annotation>				<xs:documentation xml:lang="en">Identification of the inventory group. The InventoryGrouping  Code assigned by the receiving system  in response to a notification of a new inventory grouping.</xs:documentation>			</xs:annotation>		</xs:attribute>	</xs:complexType></xs:schema>