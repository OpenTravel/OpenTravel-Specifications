<?xml version="1.0" encoding="UTF-8"?><!-- edited with XML Spy v4.2 (http://www.xmlspy.com) by Michael Schreiber (Insuractive, Inc.) --><xs:schema targetNamespace="http://www.opentravel.org/OTA/2002/08" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.opentravel.org/OTA/2002/08" elementFormDefault="qualified" version="2002A">	<xs:include schemaLocation="OTA_InsuranceCommonTypes.xsd"/>	<xs:element name="OTA_InsuranceBookRQ">		<xs:annotation>			<xs:documentation>Container for insurance plan book requests</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="POS" type="POS_Type" minOccurs="0">					<xs:annotation>						<xs:documentation>Container for any Point-of-sale information</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="PlanForBookRQ" maxOccurs="unbounded">					<xs:annotation>						<xs:documentation>Contains a booking request for a single insurance plan</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="InsCoverageType">								<xs:sequence>									<xs:element name="InsuranceCustomer" type="InsuranceCustomerType"/>									<xs:element name="UniqueId" type="UniqueIdType" minOccurs="0">										<xs:annotation>											<xs:documentation>Unique identifier associated with booking request message</xs:documentation>										</xs:annotation>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>			</xs:sequence>			<xs:attributeGroup ref="OTA_PayloadStdAttributes"/>		</xs:complexType>	</xs:element></xs:schema>