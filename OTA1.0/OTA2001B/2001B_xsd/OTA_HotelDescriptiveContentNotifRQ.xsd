			<xs:simpleContent>
				<xs:restriction base="xs:string">
					<xs:enumeration value="AV_Equip"/>
					<xs:enumeration value="AV_Technician"/>
					<xs:enumeration value="Blackboards"/>
					<xs:enumeration value="BulletinBoard"/>
