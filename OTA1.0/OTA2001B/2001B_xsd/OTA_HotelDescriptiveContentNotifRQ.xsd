<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.opentravel.org/OTA" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.opentravel.org/OTA" elementFormDefault="qualified">
	<xs:include schemaLocation="OTA_v2ent.xsd"/>
	<xs:include schemaLocation="DOWPattern.xsd"/>
	<xs:include schemaLocation="DateTimeSpan.xsd"/>
	<xs:include schemaLocation="HotelReference.xsd"/>
	<xs:include schemaLocation="CancelPenalties.xsd"/>
	<xs:element name="AbsoluteDeadline">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines the absolute deadline.  Either this or the offset attributes may be used.</xs:documentation>
		</xs:annotation>
		<xs:complexType/>
	</xs:element>
	<xs:element name="AcceptedPaymtForm">
		<xs:annotation>
			<xs:documentation xml:lang="en">The AcceptedPaymtForms class defines the policy regarding forms of payment accepted by the hotel. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CreditCardCodes" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="PaymtMethodType" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">An enumerated type that defines a method of payment for a hotel stay. </xs:documentation>
					<xs:documentation xml:lang="en">Values: 1 = Cash, 2 = Credit Card, 3= Debit Card, 4 = Voucher, 5 = Check, 6 = Other.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Cash"/>
						<xs:enumeration value="CreditCard"/>
						<xs:enumeration value="DebitCard"/>
						<xs:enumeration value="Voucher"/>
						<xs:enumeration value="Check"/>
						<xs:enumeration value="Other"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="PaymtFormUseType" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">An enumerated type that defines a form of payment for a hotel stay. </xs:documentation>
					<xs:documentation xml:lang="en">Values: 1 = Guarantee, 2 = Deposit, 3 = Paymt.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Guarantee"/>
						<xs:enumeration value="Deposit"/>
						<xs:enumeration value="Paymt"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AcceptedPaymtForms">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AcceptedPaymtForm" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AffiliationInfo">
		<xs:annotation>
			<xs:documentation xml:lang="en">The AffiliationInfo object is used to identify the brand or franchise affiliations of the hotel. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DistribSystems"/>
				<xs:element ref="Brands"/>
				<xs:element ref="LoyalPrograms"/>
				<xs:element ref="Awards"/>
			</xs:sequence>
			<xs:attribute name="LastUpdated" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The date and time when the affiliation information for this hotel was last updated. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AmenityInfo">
		<xs:annotation>
			<xs:documentation xml:lang="en">The AmenityInfo class that describes the room amenities and services at the hotel.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HotelAmenities"/>
				<xs:element ref="RoomAmenities"/>
				<xs:element ref="RecreationalSrvcs"/>
				<xs:element ref="BusinessSrvcs"/>
				<xs:element ref="PersonalSrvcs"/>
			</xs:sequence>
			<xs:attribute name="LastUpdated" type="xs:dateTime" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ArchitecturalStyleCode" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">A collection of ArchitecturalStyleCode strings used to help describe the architectural style of the hotel facility.  The list of codes is maintained by HEDNA.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ArchitecturalStyleCodes">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ArchitecturalStyleCode" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Attraction">
		<xs:annotation>
			<xs:documentation xml:lang="en">A collection of Attraction objects identifying the local interest attractions in the vicinity of this hotel facility.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ContactElectronicAddresses" minOccurs="0"/>
				<xs:element ref="ContactName" minOccurs="0"/>
				<xs:element ref="AttractionDates" minOccurs="0"/>
				<xs:element ref="PropertyCourtesyPhone" minOccurs="0"/>
				<xs:element ref="AttractionDescription"/>
				<xs:element ref="RelatedRefPoint"/>
				<xs:element ref="AttractionURL"/>
			</xs:sequence>
			<xs:attribute name="AttractionCategoryCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The code for the type of attraction in the proximity of the hotel facility.  This code list will be maintained by HEDNA.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AttractionName" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The name of the local attraction. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AttractionFee" type="xs:float" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AttractionDates">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DateTimeSpan" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AttractionDescription" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">The text description of the attraction. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AttractionURL" type="xs:anyURI">
		<xs:annotation>
			<xs:documentation xml:lang="en">The universal resource locator (URL) for the attraction as found on the Internet. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Attractions">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Attraction" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="LastUpdated" type="xs:dateTime" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AvailableCapacities">
		<xs:annotation>
			<xs:documentation xml:lang="en">The collection of MeetingRoomCapacity objects that defines the number of people permitted in a meeting room. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MeetingRoomCapacity" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AvailableMealCategoryCode" type="xs:string"/>
	<xs:element name="AvailableMealCategoryCodes">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AvailableMealCategoryCode" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AvailableMealTimeCodes" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">A collection of strings indicating the categories or types of meals available at the hotel restaurant. This code list will be maintained by HEDNA.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Award">
		<xs:complexType>
			<xs:attribute name="AwardProvider" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The name of the award or ratings provider. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Rating" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The actual award or rating received by the hotel facility. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Awards">
		<xs:annotation>
			<xs:documentation xml:lang="en">The Award class defines all of the award programs used by this hotel.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Award" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BeverageCode" type="xs:string"/>
	<xs:element name="BeverageCodes">
		<xs:annotation>
			<xs:documentation xml:lang="en">A collection of strings indicating the beverage codes for this restaurant. This code list will be maintained by HEDNA.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BeverageCode" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Brand">
		<xs:annotation>
			<xs:documentation xml:lang="en">The Brand class defines the brand affiliations of this hotel.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BrandURL"/>
			</xs:sequence>
			<xs:attribute name="BrandCategoryCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The code for the type of brand affiliated with the hotel facility.  This code list will be maintained by HEDNA.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MarketingBrand" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">Name of brand, marketing chain, representation company or other affiliation(s).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="BrandURL" type="xs:anyURI">
		<xs:annotation>
			<xs:documentation xml:lang="en">The universal resource locator (URL) for the marketing brand as found on the Internet. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Brands">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Brand" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BriefDescription" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">A text description to explain additional descriptive information about the hotel facility. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BusinessSrvc">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BusinessSrvcDescription"/>
				<xs:element ref="PositionFromHotel"/>
				<xs:element ref="OperationSchedules"/>
				<xs:element ref="UsualCharge"/>
			</xs:sequence>
			<xs:attribute name="BusinessSrvcCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The code for the type of business service offered in a room at a hotel facility.  This code list will be maintained by HEDNA.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AvailableToAnyGuest" type="xs:boolean" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">This flag indicates whether the business service is available to all guests or only to certain guests at the facility. </xs:documentation>
					<xs:documentation xml:lang="en">Values: 0 = (No) Not Available to All Guests, 1 = (Yes) Available to All Guests.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="InvCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">Identification code of the business service for inventory and booking purposes if the service is an inventoriable item.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="BusinessSrvcDescription" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">The text information describing the business service. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BusinessSrvcs">
		<xs:annotation>
			<xs:documentation xml:lang="en">The BusinessSrvc class provides the codes and description of business services available at the hotel facility. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BusinessSrvc" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ClosedSeasons">
		<xs:annotation>
			<xs:documentation xml:lang="en">A collection of date and time periods when the hotel facility is closed. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DateTimeSpan" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactElectronicAddresses">
		<xs:complexType/>
	</xs:element>
	<xs:element name="ContactInfo">
		<xs:annotation>
			<xs:documentation xml:lang="en">The ContactInfo class is used to define the contacts for consumers and technical people at the hotel facility, including various telephone numbers and e-mail addresses, or any respective attribute or affiliation for the hotel.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Contacts"/>
				<xs:element ref="HotelAddresses"/>
				<xs:element ref="HotelElectronicAddresses"/>
				<xs:element ref="HotelPhoneNmbrs"/>
			</xs:sequence>
			<xs:attribute name="ContactProfileID" type="xs:string" use="required"/>
			<xs:attribute name="ContactProfileType" type="xs:string" use="required"/>
			<xs:attribute name="LastUpdated" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The date and time when the contact information for this hotel was last updated. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactName">
		<xs:annotation>
			<xs:documentation xml:lang="en">The contact name for the hotel facility. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="NameInfo" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Contacts">
		<xs:annotation>
			<xs:documentation xml:lang="en">The HotelContact class defines the contact information for a hotel facility.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HotelContacts"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CreditCardCode" type="xs:string"/>
	<xs:element name="CreditCardCodes">
		<xs:annotation>
			<xs:documentation xml:lang="en">A collection of credit card code strings identifying the credit cards accepted for payment at the hotel.  This attribute is used if the PaymtMethodType is Credit Card, Debit Card, or Voucher. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CreditCardCode" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Description" type="xs:string"/>
	<xs:element name="DistribSystem">
		<xs:annotation>
			<xs:documentation xml:lang="en">The DistribSystem objects defining the affiliated distribution systems for a hotel.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DistribSystemHotelReference"/>
				<xs:element ref="DateTimeSpan"/>
			</xs:sequence>
			<xs:attribute name="DistribSystemCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">A code that serves as the unique identifier for the distribution system or booking engine, such as the specific GDS codes. This code list will be maintained by HEDNA.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="DistribSystemHotelReference">
		<xs:annotation>
			<xs:documentation xml:lang="en">A HotelReference object that defines a hotel facility included in the distribution system.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="HotelCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The code that uniquely identifies a single hotel property. The hotel code is decided between vendors.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ChainCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The code that identifies a hotel chain or management group. The hotel chain code is decided between vendors.  This attribute is optional if the hotel is an independent property that can be identified by the HotelCode attribute.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BrandCode" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="DistribSystems">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DistribSystem" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DrivingDirectionsFromRefPoint" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">The information describing the driving directions from the reference point to the hotel facility. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DrivingDirectionsToRefPoint" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">The information describing the driving directions to the reference point from the hotel facility. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ElectronicAddress">
		<xs:annotation>
			<xs:documentation xml:lang="en">The ElectronicAddress class is used to store pager phone numbers, email addresses and web page addresses (URL, universal resource locator). The ElectronicAddress class is defined in the Central Res Systems' Profile Synch standard.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="ElectronicAddressType" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">Valid values: E-mail, Pager, Web Page.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="EAddress" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The electronic address string, such as e-mail address or (.URL) web page location. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FacilityInfo">
		<xs:annotation>
			<xs:documentation xml:lang="en">The FacilityInfo class that describes the facilities provided at the hotel, including meeting rooms restaurants.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MeetingSrvcInfo"/>
				<xs:element ref="MeetingRooms"/>
				<xs:element ref="GuestRooms"/>
				<xs:element ref="RestaurantSrvcInfo"/>
				<xs:element ref="RestaurantSrvcInfoCodes"/>
				<xs:element ref="Restaurants"/>
				<xs:element ref="SecurityFeatures"/>
				<xs:element ref="PhyslyChallengedFeatures"/>
			</xs:sequence>
			<xs:attribute name="LastUpdated" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The date and time when this hotel information was last updated in the system. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="GeographicalInfo">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines well-known locations and attractions of local interest in the geographical area of the hotel facility.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PointsOfInterest"/>
				<xs:element ref="Attractions"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GuestRoom">
		<xs:complexType>
			<xs:attribute name="Code" type="xs:string" use="required"/>
			<xs:attribute name="CodeRequired" type="xs:string" use="required"/>
			<xs:attribute name="CodeContext" type="xs:string" use="required"/>
			<xs:attribute name="Composite" type="xs:string" use="required"/>
			<xs:attribute name="FirstFloor" type="xs:string" use="required"/>
			<xs:attribute name="MaxOccupancy" type="xs:int" use="required"/>
			<xs:attribute name="MaxRollaways" type="xs:int" use="required"/>
			<xs:attribute name="NonSmoking" type="xs:string" use="required"/>
			<xs:attribute name="Quality" type="xs:string" use="required"/>
			<xs:attribute name="Configuration" type="xs:string" use="required"/>
			<xs:attribute name="Size" type="xs:int" use="required"/>
			<xs:attribute name="SizeMeasurement" type="xs:string" use="required"/>
			<xs:attribute name="Type" type="xs:string" use="required"/>
			<xs:attribute name="TypeImplied" type="xs:string" use="required"/>
			<xs:attribute name="TypeClassName" type="xs:string" use="required"/>
			<xs:attribute name="TypeCount" type="xs:int" use="required"/>
			<xs:attribute name="TypeName" type="xs:string" use="required"/>
			<xs:attribute name="StandardNumBeds" type="xs:int" use="required"/>
			<xs:attribute name="StandardOccupancy" type="xs:int" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="GuestRooms">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="GuestRoom" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="HotelAddress">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PostalAddress" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="AddressPurposeCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en">The code that identifies specific use for the address in correspondence with the hotel facility.  This code list will be maintained by HEDNA. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="HotelAddresses">
		<xs:annotation>
			<xs:documentation xml:lang="en">The HotelAddress class defines the addresses at this hotel facility.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HotelAddress"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="HotelAmenities">
		<xs:annotation>
			<xs:documentation xml:lang="en">The HotelAmenity class provides the codes and description of amenities in the hotel facility. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HotelAmenity"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="HotelAmenity">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="OperationSchedules"/>
				<xs:element ref="UsualCharge"/>
			</xs:sequence>
			<xs:attribute name="HotelAmenityCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The code for the type of amenity at a hotel facility.  This code list will be maintained by HEDNA.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="HotelAmenityDescription" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The text information describing the hotel amenity. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AvailableToAnyGuest" type="xs:boolean" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">This flag indicates whether the amenity is available to any guest or only to certain guests at the facility.</xs:documentation>
					<xs:documentation xml:lang="en">Values: 0 = (No) Restricted Avail, 1 = (Yes) Available to All Guests.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="InvCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">Identification code of the amenity for inventory and booking purposes if the amenity is an inventoriable item.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="HotelCategoryCode" type="xs:string"/>
	<xs:element name="HotelCategoryCodes">
		<xs:annotation>
			<xs:documentation xml:lang="en">A collection of HotelCategoryCode strings indicating which primary and/or secondary types of properties are described.  The HotelCategoryCodes are maintained by HEDNA.  </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HotelCategoryCode" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="HotelContact">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ContactName"/>
			</xs:sequence>
			<xs:attribute name="GenderType" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The gender of the contact person in the ContactName attribute. This may be useful for purposes of correspondence to the hotel. </xs:documentation>
					<xs:documentation xml:lang="en">Values: 0 = Unknown, 1 = Male, 2 = Female.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Unknown"/>
						<xs:enumeration value="Female"/>
						<xs:enumeration value="Male"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ContactCategoryCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The code that defines the type of contact information for the hotel facility. This code list will be maintained by HEDNA. Types of contacts include: Main chain contact, Res, Marketing, Sales, Technical Support, Cust Support, etc.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ContactSrvcCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The information describing the contact service code for a hotel facility. (e.g., who to contact about golf tours). This code list will be maintained by HEDNA.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ContactSrvcCategoryCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The category code for the service contact at a hotel facility This provides a reference table of how to interpret the Contact Srvc Code. This code list will be maintained by HEDNA.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ContactLocation" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The location of the hotel contact. e.g.: at the hotel property, at a central office, etc. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ContactCorporatePosition" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The title or corporate position of the contact. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OKToPublish" type="xs:boolean" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates whether the contact information can be published externally (on a web site for the general public) or whether this is strictly a contact to be used in conjunction with the data feed (limited use). </xs:documentation>
					<xs:documentation xml:lang="en">Values: 0 = No, Limited Publication, or 1 = Yes, OK for External Publication.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="HotelContacts">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HotelContact" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="HotelContentType">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="AffiliationInfo"/>
				<xs:enumeration value="AmenityInfo"/>
				<xs:enumeration value="Contactinfo"/>
				<xs:enumeration value="FacilityInfo"/>
				<xs:enumeration value="GeographicalInfo"/>
				<xs:enumeration value="HotelInfo"/>
				<xs:enumeration value="MultimediaInfo"/>
				<xs:enumeration value="PolicyInfo"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="HotelContentTypes">
		<xs:annotation>
			<xs:documentation xml:lang="en">A collection of HotelContentTypes indicating which types of content are included in this object. Implementation Note: If the types are not set, then this indicates "no change" in the previous information when this object is pushed to the server.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HotelContentType" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="HotelDescriptiveContent">
		<xs:annotation>
			<xs:documentation xml:lang="en">The HotelDescriptiveContent element contains the descriptive information about a hotel property.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HotelReference"/>
				<xs:element ref="HotelContentTypes"/>
				<xs:element ref="HotelInfo"/>
				<xs:element ref="AmenityInfo"/>
				<xs:element ref="FacilityInfo"/>
				<xs:element ref="PolicyInfo"/>
				<xs:element ref="GeographicalInfo"/>
				<xs:element ref="AffiliationInfo"/>
				<xs:element ref="ContactInfo"/>
				<xs:element ref="MultimediaInfo"/>
			</xs:sequence>
			<xs:attribute name="LanguageID" type="xs:language" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">Identification of the requested language to be used for the hotel descriptive information. ISO standard for human-readable languages.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="OTA_HotelDescriptiveContentNotifRQ">
		<xs:annotation>
			<xs:documentation xml:lang="en">The OTA_HotelDescriptiveContentNotifRQ is the message that sends the notification of the rules and restrictions affecting the availability status of the hotel.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HotelReference"/>
				<xs:element ref="HotelDescriptiveContent"/>
			</xs:sequence>
			<xs:attributeGroup ref="OTA_PayloadStdAttributes">
				<xs:annotation>
					<xs:documentation xml:lang="en">This element defines standard attributes that appear on the root element for all OTA payloads.</xs:documentation>
				</xs:annotation>
			</xs:attributeGroup>
			<xs:attribute name="LanguageCode" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="HotelDescriptiveContents">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HotelDescriptiveContent"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
