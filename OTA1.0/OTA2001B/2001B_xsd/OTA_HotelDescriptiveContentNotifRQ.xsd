					<xs:enumeration value="Copier"/>
					<xs:enumeration value="Decorator"/>
					<xs:enumeration value="DirectionSigns"/>
					<xs:enumeration value="DiscPlayers"/>
					<xs:enumeration value="EaselsOrTablets"/>
					<xs:enumeration value="Electrician"/>
					<xs:enumeration value="FirstAid"/>
					<xs:enumeration value="FlipChartAndMarkers"/>
					<xs:enumeration value="HighSpeedInternetAccess"/>
					<xs:enumeration value="ISDN_Line"/>
					<xs:enumeration value="InstalledStage"/>
					<xs:enumeration value="LCDProjector"/>
					<xs:enumeration value="LCD_Panel"/>
					<xs:enumeration value="Laborers"/>
					<xs:enumeration value="Locksmith"/>
					<xs:enumeration value="MessengerSrvc"/>
					<xs:enumeration value="Microphone"/>
					<xs:enumeration value="MusiciansEntertainers"/>
					<xs:enumeration value="NameCardsHolders"/>
					<xs:enumeration value="NotaryPublic"/>
					<xs:enumeration value="Overhead"/>
					<xs:enumeration value="PA_System"/>
					<xs:enumeration value="PensPencilsPads"/>
					<xs:enumeration value="Photographer"/>
					<xs:enumeration value="Podium"/>
					<xs:enumeration value="PollingDevices"/>
					<xs:enumeration value="PortableStage"/>
					<xs:enumeration value="PrintingSrvcs"/>
					<xs:enumeration value="Projector"/>
					<xs:enumeration value="ProjectorAndScreen"/>
					<xs:enumeration value="RearScreen"/>
					<xs:enumeration value="RegistrationSrvcs"/>
					<xs:enumeration value="SecurityGuards"/>
					<xs:enumeration value="SeparateEntrance"/>
					<xs:enumeration value="SeparateRegistration"/>
					<xs:enumeration value="SpotLights"/>
					<xs:enumeration value="StackChairs"/>
					<xs:enumeration value="Stenographer"/>
					<xs:enumeration value="TV"/>
					<xs:enumeration value="TVProductionSrvcProvider"/>
					<xs:enumeration value="TablesAvailable"/>
					<xs:enumeration value="Tours"/>
					<xs:enumeration value="VCRRental"/>
					<xs:enumeration value="VideoCamera"/>
					<xs:enumeration value="VideoConferencing"/>
					<xs:enumeration value="VideoConferencingSrvcProvider"/>
					<xs:enumeration value="WalkieTalkiesRadio"/>
					<xs:enumeration value="WalkieTalkiesRadioSrvcProvider"/>
					<xs:enumeration value="WallCharts"/>
					<xs:enumeration value="Whiteboards"/>
