			<xs:sequence>
				<xs:element ref="HotelElectronicAddress" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="HotelInfo">
		<xs:annotation>
			<xs:documentation xml:lang="en">The HotelInfo class that describes general information about the hotel.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HotelInfoCodes"/>
				<xs:element ref="Renovations"/>
				<xs:element ref="HotelName"/>
				<xs:element ref="HotelCategoryCodes"/>
				<xs:element ref="SegmentCategoryCodes"/>
				<xs:element ref="LocationCategoryCodes"/>
				<xs:element ref="LocationDescription"/>
				<xs:element ref="ArchitecturalStyleCodes"/>
				<xs:element ref="ClosedSeasons"/>
				<xs:element ref="Position"/>
				<xs:element ref="PositionFromCity"/>
				<xs:element ref="PositionFromAirport"/>
				<xs:element ref="PositionFromRailStation"/>
				<xs:element ref="BriefDescription"/>
			</xs:sequence>
			<xs:attribute name="HotelShortName" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The shortened (abbreviated) hotel name that identifies the hotel facility. This attribute could be the name that it is generally known by, or used in a search for the hotel.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="HotelURL" type="xs:anyURI" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en">The universal resource locator (URL) for the hotel as defined on the Internet. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="WhenBuilt" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The date that the hotel facility was built. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TotalRoomCount" type="xs:byte" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The total room count for the hotel facility. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PhyslyChallengedRoomCount" type="xs:byte" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The total count of rooms with physically-challenged attributes at the hotel facility. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NonSmokingRoomCount" type="xs:int" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The total count of rooms designated as non-smoking at the hotel facility. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SuiteAndApartmentCount" type="xs:int" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en">The total count of rooms that are classified as suites or apartments at the hotel facility. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BungalowAndVillaCount" type="xs:int" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en">The total count of rooms that are classified as bungalows or villas at the hotel facility. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FloorCount" type="xs:byte" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">The number of floors at the hotel facility. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NearestIntersection" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en">The text description for the nearest cross streets to the hotel facility. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NearestHighway" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en">The text description for the nearest highway to the hotel facility. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NearestHighwayExit" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en">The text description for the closest highway exit to the hotel facility. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NearestMajorCity" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en">The name of the closest major city to the hotel facility. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NearestAirport" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en">The airport code for the major airport closest to the hotel facility.  These codes are defined and maintained by IATA (Intl. Air Transport Assn.).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NearestRailStation" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="en">The railway code for the nearest railroad station to the hotel facility.  These codes are defined and maintained by Amtrak (United States).  </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="LastUpdated" type="xs:dateTime" use="optional"/>
			<xs:attribute name="ExecutiveFloor" type="xs:string" use="required"/>
			<xs:attribute name="NumRoomsThatWork" type="xs:string" use="optional"/>
			<xs:attribute name="NumAvailableRms" type="xs:int" use="required"/>
			<xs:attribute name="NumAvailableSuites" type="xs:int" use="required"/>
			<xs:attribute name="NumDoubleBedRms" type="xs:int" use="required"/>
			<xs:attribute name="NumFloors" type="xs:int" use="required"/>
			<xs:attribute name="NumKingBedRms" type="xs:short" use="required"/>
			<xs:attribute name="NumNonSmoking" type="xs:short" use="required"/>
			<xs:attribute name="NumPhysRms" type="xs:short" use="required"/>
			<xs:attribute name="NumPhysSuites" type="xs:short" use="required"/>
			<xs:attribute name="NumQueenBedRms" type="xs:short" use="required"/>
			<xs:attribute name="TotalAvailableRms" type="xs:short" use="required"/>
			<xs:attribute name="TotalNmbrPenthouses" type="xs:short" use="required"/>
			<xs:attribute name="TotalNmbrStudios" type="xs:short" use="required"/>
			<xs:attribute name="TotalPhysRms" type="xs:short" use="required"/>
			<xs:attribute name="AreaUnits" type="xs:string" use="required"/>
			<xs:attribute name="AreaWeather" type="xs:string" use="optional"/>
			<xs:attribute name="CurrencyCode" type="xs:string" use="required"/>
			<xs:attribute name="InterfaceCompliance" type="xs:string" use="optional"/>
			<xs:attribute name="PMSSystem" type="xs:string" use="optional"/>
			<xs:attribute name="PropertyEmail" type="xs:string" use="optional"/>
			<xs:attribute name="ChainEmail" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="HotelInfoCode" type="xs:string"/>
	<xs:element name="HotelInfoCodes">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HotelInfoCode" maxOccurs="unbounded"/>
