			<xs:attribute name="RoomName" type="xs:string"/>
			<xs:attribute name="School" type="xs:boolean"/>
			<xs:attribute name="Sort" type="xs:short"/>
			<xs:attribute name="StandardOccupancy" type="xs:int"/>
			<xs:attribute name="Theatre" type="xs:boolean"/>
			<xs:attribute name="Units" type="xs:string"/>
			<xs:attribute name="UShape" type="xs:boolean"/>
			<xs:attribute name="Width" type="xs:short"/>
