<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.opentravel.org/OTA" xmlns="http://www.opentravel.org/OTA" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:include schemaLocation="OTA_v2ent.xsd"/>
	<xs:include schemaLocation="Privacy.xsd"/>
	<xs:group name="SeatingPref">
		<xs:sequence>
			<xs:element ref="SeatingPref"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="UniqueId">
		<xs:sequence>
			<xs:element ref="UniqueId"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="CompanyName">
		<xs:sequence>
			<xs:element ref="CompanyName"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="Telephone">
		<xs:sequence>
			<xs:element ref="Telephone"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="PersonName">
		<xs:sequence>
			<xs:element ref="PersonName"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="Profile">
		<xs:annotation>
			<xs:documentation>Root element for profile content.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Accesses" minOccurs="0"/>
				<xs:element ref="Customer" minOccurs="0"/>
				<xs:element ref="Preferences" minOccurs="0"/>
				<xs:element ref="CompanyInfo" minOccurs="0"/>
				<xs:element ref="Affiliations" minOccurs="0"/>
				<xs:element ref="Agreements" minOccurs="0"/>
				<xs:element ref="TPA_Extensions" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="ShareAllSynchInd" default="No">
				<xs:annotation>
					<xs:documentation>Permission for sharing all data in profile for synchronization of profiles held by other travel service providers.
						</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ShareAllMarketInd" default="No">
				<xs:annotation>
					<xs:documentation>Permission for sharing all data in profile for marketing purposes.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ProfileType" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Customer"/>
						<xs:enumeration value="GDS"/>
						<xs:enumeration value="Corporation"/>
						<xs:enumeration value="Trvl_Agnt"/>
						<xs:enumeration value="Wholesaler"/>
						<xs:enumeration value="Group"/>
						<xs:enumeration value="TourOperator"/>
						<xs:enumeration value="CRO"/>
						<xs:enumeration value="RepCompany"/>
						<xs:enumeration value="InternetBroker"/>
						<xs:enumeration value="Airline"/>
						<xs:enumeration value="Hotel"/>
						<xs:enumeration value="CarRental"/>
						<xs:enumeration value="CruiseLine"/>
						<xs:enumeration value="Employee"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Accesses">
		<xs:annotation>
			<xs:documentation>Element to capture creation and last update data.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Access" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="CreateDateTime" type="xs:string">
				<xs:annotation>
					<xs:documentation>Month, day, year and optionally hour, minute, second of day the profile originated, in ISO 8601 format.
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Access">
		<xs:annotation>
			<xs:documentation>Describes the action taken on the record.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AccessPerson" minOccurs="0"/>
				<xs:element ref="AccessComment" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="ActionType">
				<xs:annotation>
					<xs:documentation>Type of action taken on the profile. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Create"/>
						<xs:enumeration value="Read"/>
						<xs:enumeration value="Update"/>
						<xs:enumeration value="Delete"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ActionDateTime" type="xs:string">
				<xs:annotation>
					<xs:documentation>Month, day, year and optionally in hour, minute, second of day in ISO 8601 format of the action taken on the profile.
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ActionSystemId" type="xs:string">
				<xs:annotation>
					<xs:documentation>The identifier of the system that performed the action on the profile record. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AccessPerson" type="xs:string">
		<xs:annotation>
			<xs:documentation>Name of individual who originated or updated record.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AccessComment" type="xs:string">
		<xs:annotation>
			<xs:documentation>Free text comment added by the person accessing the profile record. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Customer">
		<xs:annotation>
			<xs:documentation>Contains basic data on the customer's identity, location, relationships, finances, memberships, etc.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:sequence>
						<xs:element ref="PersonName" maxOccurs="unbounded"/>
						<xs:element ref="TelephoneInfo" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="Email" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:sequence>
						<xs:element ref="TelephoneInfo" maxOccurs="unbounded"/>
						<xs:element ref="Email" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:choice>
						<xs:element ref="Email" maxOccurs="unbounded"/>
					</xs:choice>
				</xs:choice>
				<xs:element ref="AddressInfo" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="URL" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="CitizenCountryName" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PhysChallName" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PetInfo" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PaymentForm" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="RelatedTraveler" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ContactPerson" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Document" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="CustLoyalty" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="Gender">
				<xs:annotation>
					<xs:documentation>Indicates the gender of the person identified in this profile. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Male"/>
						<xs:enumeration value="Female"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Deceased" default="No">
				<xs:annotation>
					<xs:documentation>Notes if individual in this profile has died. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="LockoutType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Indicates reason for locking out record, such as Emergency, Incident, etc. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="BirthDate" type="xs:string">
				<xs:annotation>
					<xs:documentation>Indicates self-professed date of birth, in ISO 8901 prescribed format.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Currency" type="xs:string">
				<xs:annotation>
					<xs:documentation>Type of funds preferred for reviewing monetary values, in ISO 4217 codes.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="PersonName">
		<xs:annotation>
			<xs:documentation>Names of the individual, includes former, nicknames, and alternate names.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="NamePrefix" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="GivenName" minOccurs="0"/>
				<xs:element ref="MiddleName" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Surname"/>
				<xs:element ref="NameSuffix" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="NameTitle" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="DefaultInd">
				<xs:annotation>
					<xs:documentation>Indicates that the receiving system should assume the default value if the user specifies no overriding
						 value or action.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="NameType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Type of name of the individual, such as former, nickname, alternate or alias name. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="NamePrefix" type="xs:string">
		<xs:annotation>
			<xs:documentation>Salutation of honorific. (e.g., Mr. Mrs., Ms., Miss, Dr., etc.)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GivenName" type="xs:string">
		<xs:annotation>
			<xs:documentation>Given name, first name or names.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="MiddleName" type="xs:string">
		<xs:annotation>
			<xs:documentation>Middle name or names.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Surname" type="xs:string">
		<xs:annotation>
			<xs:documentation>Family name, last name. Required for identification of the individual in the profile. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="NameSuffix" type="xs:string">
		<xs:annotation>
			<xs:documentation>Suffix (e.g., Jr., Sr., III, Ret., Esq., etc.)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="NameTitle" type="xs:string">
		<xs:annotation>
			<xs:documentation>Degree or honors (e.g., Ph.D., M.D., etc.)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TelephoneInfo">
		<xs:annotation>
			<xs:documentation>Information about the telephone numbers of the individual in the profile.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Telephone"/>
			</xs:sequence>
			<xs:attribute name="DefaultInd">
				<xs:annotation>
					<xs:documentation>Indicates that the receiving system should assume the default value if the user specifies no overriding value or action.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="PhoneUseType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Describes the type of telephone number, in the context of its general use, such as Home, Business, Emergency Contact, 
					Travel Arranger, Day, Evening, etc.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Telephone">
		<xs:annotation>
			<xs:documentation>Telephone numbers for the parties in the profile.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="PhoneTechType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Indicates type of technology associated with this telephone number, such as Voice, Data, Fax, Pager, Cell, TTY, etc.
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CountryAccessCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>Code assigned by telecommunications authorities for international country access identifier.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AreaCityCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Code assigned for telephones in a specific region, city, or area.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PhoneNumber" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Telephone number assigned to a single location.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Extension" type="xs:string">
				<xs:annotation>
					<xs:documentation>Extension to reach a specific party at the phone number.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PIN" type="xs:string">
				<xs:annotation>
					<xs:documentation>Additional codes used for pager or telephone access rights.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Email">
		<xs:annotation>
			<xs:documentation>Electronic email addresses, in IETF specified format.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="DefaultInd">
						<xs:annotation>
							<xs:documentation>Indicates that the receiving system should assume the default value if the user specifies no overriding value or action.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Yes"/>
								<xs:enumeration value="No"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attributeGroup ref="Privacy"/>
					<xs:attribute name="EmailType" type="xs:string">
						<xs:annotation>
							<xs:documentation>Defines the purpose of the Email address, such as personal, business, listserve, etc.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AddressInfo">
		<xs:annotation>
			<xs:documentation>Information about the addresses in the profile that identify locations for specific purposes.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Address"/>
			</xs:sequence>
			<xs:attribute name="DefaultInd">
				<xs:annotation>
					<xs:documentation>Indicates that the receiving system should assume the default value if the user specifies no overriding value or action.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="AddressType">
				<xs:annotation>
					<xs:documentation>Defines what the purpose of the address is, such as home, business, mailing, delivery, 
						shipping/receiving address, etc.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Home"/>
						<xs:enumeration value="Business"/>
						<xs:enumeration value="Delivery"/>
						<xs:enumeration value="Mailing"/>
						<xs:enumeration value="Billing"/>
						<xs:enumeration value="CreditCard"/>
						<xs:enumeration value="Other"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Address">
		<xs:annotation>
			<xs:documentation>Defines a physical address of parties in the profile.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="StreetNmbr" minOccurs="0"/>
				<xs:element ref="BldgRoom" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AddressLine" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="CityName" minOccurs="0"/>
				<xs:element ref="StateProv" minOccurs="0"/>
				<xs:element ref="CountryName" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="StreetNmbr">
		<xs:annotation>
			<xs:documentation>Street name; number on street.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="PO_Box" type="xs:string">
						<xs:annotation>
							<xs:documentation>Defines a Post Office Box number.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="BldgRoom" type="xs:string">
		<xs:annotation>
			<xs:documentation>Building name, room, apartment, or suite number.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AddressLine">
		<xs:annotation>
			<xs:documentation>Additional line of an address as needed to define a physical location.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="ParsedInd">
						<xs:annotation>
							<xs:documentation>Indicates if the data in the AddressLine field is parsed.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Yes"/>
								<xs:enumeration value="No"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CityName">
		<xs:annotation>
			<xs:documentation>Name of city or town.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="PostalCode" type="xs:string">
						<xs:annotation>
							<xs:documentation>Identifier for location assigned by postal authorities.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="StateProv">
		<xs:annotation>
			<xs:documentation>State, province, or region name or code needed to identify location.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="StateCode" type="xs:string">
						<xs:annotation>
							<xs:documentation>The postal service standard code or abbreviation for the state, province, or region.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CountryName">
		<xs:annotation>
			<xs:documentation>The name of the country specified in the address.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="CountryCode" type="xs:string">
						<xs:annotation>
							<xs:documentation>ISO 3166 code for country in address.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="URL">
		<xs:annotation>
			<xs:documentation>Web site address, in IETF specified format.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="DefaultInd">
						<xs:annotation>
							<xs:documentation>Indicates that the receiving system should assume the default value if the user specifies no overriding 
								value or action.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Yes"/>
								<xs:enumeration value="No"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attributeGroup ref="Privacy"/>
					<xs:attribute name="URL_Type" type="xs:string">
						<xs:annotation>
							<xs:documentation>Defines the purpose of the URL address, such as personal, business, public, etc.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CitizenCountryName">
		<xs:annotation>
			<xs:documentation>Self-professed country or countries that customer claims for citizenship.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="DefaultInd">
						<xs:annotation>
							<xs:documentation>Indicates that the receiving system should assume the default value if the user specifies no overriding value or action.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Yes"/>
								<xs:enumeration value="No"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="CountryCode" type="xs:string">
						<xs:annotation>
							<xs:documentation>ISO 3166 code for country indicated for citizenship.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="PhysChallName" type="xs:string">
		<xs:annotation>
			<xs:documentation>Any special physical conditions that can affect travel choices, including allergies.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PetInfo" type="xs:string">
		<xs:annotation>
			<xs:documentation>Information about pets that may accompany customer while traveling.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PaymentForm">
		<xs:annotation>
			<xs:documentation>Ways of providing funds for travel by the individual.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CreditCard" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="BankAcct" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="DirectBill" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Voucher" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="CostCenterId" type="xs:string">
				<xs:annotation>
					<xs:documentation>Identification for allocating cost of travel to  company account.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RPH" type="xs:string">
				<xs:annotation>
					<xs:documentation>A Reference Place Holder that serves as an index to the collection of payments forms stored in the profile
					 record. The RPH may be used to reference a method of payment from outside the profile.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CreditCard">
		<xs:annotation>
			<xs:documentation>Payment accounts authorizing purchases represented by plastic cards often with magnetic strips with data for identification.
				</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CardHolderName" minOccurs="0"/>
				<xs:element ref="CardIssuerName" minOccurs="0"/>
				<xs:element ref="Address" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="CardType">
				<xs:annotation>
					<xs:documentation>Indicates the type of magnetic striped card.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Credit"/>
						<xs:enumeration value="Debit"/>
						<xs:enumeration value="CentralBill"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="CreditCardCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>Code indicating the type of credit card (e.g., MasterCard, Visa, American Express, etc.)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CardNumber" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Credit card number embossed on the card.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SeriesCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>Verification digits printed on the card following the embossed number.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="EffectiveDate" type="xs:string">
				<xs:annotation>
					<xs:documentation>Date the card becomes valid for use in ISO 8601 prescribed format.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ExpireDate" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Date the card expires and can no longer be used in ISO 8601 prescribed format.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CardHolderName" type="xs:string">
		<xs:annotation>
			<xs:documentation>Name of individual cardholder as embossed on the credit/debit card.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CardIssuerName">
		<xs:annotation>
			<xs:documentation>Name of bank or organization issuing the card (e.g., alumni association, bank, fraternal organization, etc.).
				</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="IssuingBankId" type="xs:string">
						<xs:annotation>
							<xs:documentation>Code of bank issuing the card.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="BankAcct">
		<xs:annotation>
			<xs:documentation>Customer bank accounts for payments, either for paper checks or electronic funds transfer.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BankAcctName" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="BankId" type="xs:string">
				<xs:annotation>
					<xs:documentation>Code assigned by authorities to financial institutions; sometimes called bank routing number.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AcctType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Describes the bank account used for financing travel, such as: Checking, Savings, Investment, etc.
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BankAcctNumber" type="xs:string">
				<xs:annotation>
					<xs:documentation>Identifier for the account assigned by the bank.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="BankAcctName" type="xs:string">
		<xs:annotation>
			<xs:documentation>Names of the individuals as listed on the bank account.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DirectBill">
		<xs:annotation>
			<xs:documentation>Company name and location for sending invoice for remittances for travel services.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CompanyName"/>
				<xs:element ref="Address"/>
			</xs:sequence>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="DirectBill_Id" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Identifier for the organization to be billed directly for travel services.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Voucher">
		<xs:annotation>
			<xs:documentation>A form of payment using a voucher or coupon.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="EffectiveDate" type="xs:string">
						<xs:annotation>
							<xs:documentation>The date when a voucher becomes valid for use, if applicable.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ExpireDate" type="xs:string">
						<xs:annotation>
							<xs:documentation>The date when a voucher or series of coupons expires and is no longer valid.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="SeriesCode" type="xs:string">
						<xs:annotation>
							<xs:documentation>Identification of a series of coupons or vouchers identified by serial number(s).</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="RelatedTraveler">
		<xs:annotation>
			<xs:documentation>Other travel profiles associated with an individual.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="UniqueId"/>
				<xs:element ref="PersonName"/>
			</xs:sequence>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="Relation" type="xs:string">
				<xs:annotation>
					<xs:documentation>Indicates the type of relationship with the person in the profile, such as Spouse, Child, Family, Business Associate,
					 Interest Group, Medical, Security, Other, etc.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactPerson">
		<xs:annotation>
			<xs:documentation>Names of individuals to contact.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PersonName"/>
				<xs:element ref="TelephoneInfo" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AddressInfo" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Email" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="URL" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="CompanyName" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="EmployeeInfo" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="DefaultInd">
				<xs:annotation>
					<xs:documentation>Indicates that the receiving system should assume the default value if the user specifies no overriding value
						 or action. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="ContactType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Type of contact in the context of use for the travel experience; such as permanent, temporary, affiliation,
					 travel arranger, etc.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Relation" type="xs:string">
				<xs:annotation>
					<xs:documentation>Indicates the type of relationship with the person or company in the profile, such as Spouse, Child, Family, Business Associate,
					 Interest Group, Medical, Security,Other, etc.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="EmergencyFlag" type="xs:string">
				<xs:annotation>
					<xs:documentation>Indicates if this contact should be used in the case of an emergency.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RPH" type="xs:string">
				<xs:annotation>
					<xs:documentation>A Reference Place Holder that serves as an index to the collection of contacts stored in the profile record.
					 The RPH may be used to reference a contact from outside the profile, e.g.: in a reservation</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Document">
		<xs:annotation>
			<xs:documentation>Government-issued documents for travel.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DocHolderName" minOccurs="0"/>
				<xs:element ref="DocIssueAuthority" minOccurs="0"/>
				<xs:element ref="DocIssueLocation" minOccurs="0"/>
				<xs:element ref="DocLimitations" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="DocId" type="xs:string">
				<xs:annotation>
					<xs:documentation>Unique number assigned by authorities to document.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DocType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Indicates the type of travel documents held by the person.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Gender">
				<xs:annotation>
					<xs:documentation>Gender can be used to identify documents.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Male"/>
						<xs:enumeration value="Female"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="BirthDate" type="xs:string">
				<xs:annotation>
					<xs:documentation>Indicates the date of birth as indicated in the document, in ISO 8601 prescribed formatt.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="EffectiveDate" type="xs:string">
				<xs:annotation>
					<xs:documentation>Indicates the date on which the document takes effect.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ExpireDate" type="xs:string">
				<xs:annotation>
					<xs:documentation>Indicates the date after which the document is no longer valid, in ISO 8601 format.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="DocHolderName" type="xs:string">
		<xs:annotation>
			<xs:documentation>Name of person given on the document.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DocIssueAuthority" type="xs:string">
		<xs:annotation>
			<xs:documentation>Government authority issuing document. If a national document, use ISO 3166 code to indicate the code of the country
			 issuing the document.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DocIssueLocation" type="xs:string">
		<xs:annotation>
			<xs:documentation>City, state/province in which the document was issued.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DocLimitations" type="xs:string">
		<xs:annotation>
			<xs:documentation>Limitations on travel documents, such as visas for specific purposes, restrictions for entry into certain countries,
			 or corrective lenses for driver's license.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CustLoyalty">
		<xs:annotation>
			<xs:documentation>Program rewarding frequent use by accumulating credits for services provided by vendors.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="ProgramCode" type="xs:string">
						<xs:annotation>
							<xs:documentation>Identifying code for the loyalty program.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attributeGroup ref="Privacy"/>
					<xs:attribute name="SingleVendorInd">
						<xs:annotation>
							<xs:documentation>Indicates if program is affiliated with a group of related offers accumulating credits.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="SingleVndr"/>
								<xs:enumeration value="Alliance"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="MembershipId" type="xs:string">
						<xs:annotation>
							<xs:documentation>Unique identifier of the member in the program, ie. membership number, account number, etc.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="LoyalLevel" type="xs:string">
						<xs:annotation>
							<xs:documentation>Indicates special privileges in program assigned to individual.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="SignupDate" type="xs:string">
						<xs:annotation>
							<xs:documentation>Indicates when the member signed up for the loyalty program.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="EffectiveDate" type="xs:string">
						<xs:annotation>
							<xs:documentation>Indicates when privileges of membership in the loyalty program starts.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ExpireDate" type="xs:string">
						<xs:annotation>
							<xs:documentation>Indicates when membership in the loyalty program expires.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="RPH" type="xs:string">
						<xs:annotation>
							<xs:documentation>Reference or index number to be used to identify this loyalty program.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Preferences">
		<xs:annotation>
			<xs:documentation>Needs of the traveler related to travel experiences.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PrefCollection" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attributeGroup ref="Privacy"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="PrefCollection">
		<xs:annotation>
			<xs:documentation>Unique aggregation of customer travel needs.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CommonPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="VehicleRentalPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirlinesPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="HotelPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="OtherSrvcPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TPA_Extensions" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="TravelType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Identifes the category of travel for a group of preferences.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CommonPref">
		<xs:annotation>
			<xs:documentation>Travel needs associated with a collection but independent of specific travel services.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="NamePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PhonePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AddressPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PaymentFormPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="InterestPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="InsurancePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SeatingPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TicketDistribPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MediaEntertainPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PetInfoPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MealPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="LoyaltyPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SpecRequestPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="RelatedTravelerPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TPA_Extensions" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="SmokingInd">
				<xs:annotation>
					<xs:documentation>Indicates preference of smoking or no-smoking.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="PrimaryLanguage" type="xs:string">
				<xs:annotation>
					<xs:documentation>Identifes the primary language preference for the form of travel represented in a collection.
					 The human language is identified by ISO 639 codes.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AltLanguage" type="xs:string">
				<xs:annotation>
					<xs:documentation>Identifes the alternate language preferred by the customer for the form of travel specified in a collection.
					 The human language is identified by ISO 639 codes.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="VehicleRentalPref">
		<xs:annotation>
			<xs:documentation>Vehicle Rental Preferences.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LoyaltyPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="VendorPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PaymentFormPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="VehicleCoveragePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SpecRequestPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="VehicleTypePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="VehicleClassPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirConditionPref" minOccurs="0"/>
				<xs:element ref="TransmissionPref" minOccurs="0"/>
				<xs:element ref="CellPhonePref" minOccurs="0"/>
				<xs:element ref="GlobalNavPref" minOccurs="0"/>
				<xs:element ref="PhysChallEquipPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ChildSeatPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TPA_Extensions" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="PreferLevel" default="Preferred">
				<xs:annotation>
					<xs:documentation>Indicates the level of preference for the vehicle rental collection.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Only"/>
						<xs:enumeration value="Unacceptable"/>
						<xs:enumeration value="Preferred"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="SmokingInd">
				<xs:annotation>
					<xs:documentation>Indicates preference of smoking or no-smoking.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="CarRenterType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Category of renter.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="GasPrePayInd">
				<xs:annotation>
					<xs:documentation>Indicates preference for prepaid fuel option.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirlinesPref">
		<xs:annotation>
			<xs:documentation>Identifes a collection of preferences for airline travel.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LoyaltyPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="VendorPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PaymentFormPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirportOriginPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirportRoutePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirFareRestrictPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="FlightTypePref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirEquipPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirCabinPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirSrvcClassPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirSeatPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TicketDistribPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MediaEntertainPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PetInfoPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MealPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SpecRequestPref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AirSSR_Pref" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TPA_Extensions" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="Privacy"/>
			<xs:attribute name="PreferLevel" default="Preferred">
				<xs:annotation>
					<xs:documentation>Indicates level of preference for the special request.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Only"/>
						<xs:enumeration value="Unacceptable"/>
						<xs:enumeration value="Preferred"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="SmokingInd">
