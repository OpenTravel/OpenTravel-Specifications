				<xs:element ref="TravelArranger" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TravelClub" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Insurance" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TPA_Extensions" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="Privacy"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Organization">
		<xs:annotation>
			<xs:documentation>Membership organization that has travel benefits, programs, or discounts.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="OrgMemberName" minOccurs="0"/>
