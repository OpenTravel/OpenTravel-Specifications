				<xs:element ref="InternalRefNmbr" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TravelArranger" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="LoyaltyProgram" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="DefaultInd">
				<xs:annotation>
					<xs:documentation>Indicates that the receiving system should assume the default value if the user specifies no overriding 
						value or action.
						</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="OfficeType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Main office, field office, or division of the organization.</xs:documentation>
