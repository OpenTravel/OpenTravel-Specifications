							<xs:restriction base="xs:string">
								<xs:enumeration value="SingleVndr"/>
								<xs:enumeration value="Alliance"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="LoyaltyLevel" type="xs:string"/>
					<xs:attribute name="RPH" type="xs:string"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Affiliations">
