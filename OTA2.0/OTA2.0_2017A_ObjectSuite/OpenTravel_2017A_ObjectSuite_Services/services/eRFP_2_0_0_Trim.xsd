    <xsd:element name="E_RFPIdentifier" type="rfp-0200:E_RFP_ID"/>
    <xsd:element name="E_RFP" substitutionGroup="rfp-0200:E_RFPSubGrp" type="rfp-0200:E_RFP"/>
    <xsd:element name="E_RFPSummary" type="rfp-0200:E_RFP"/>
    <xsd:element name="E_RFPDetail"
        substitutionGroup="rfp-0200:E_RFPSubGrp" type="rfp-0200:E_RFP_Detail"/>
    
    <xsd:complexType name="E_RFP_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Infomation pertaining to a request for proposal (meeting room and/or sleeping room information for a group).</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">E_RFP</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Identifier" type="ota2-0200:Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">A unique identifier defined by an external authority for this object.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="E_RFPRef" type="xsd:IDREF">
                <xsd:annotation>
                    <xsd:documentation source="Description">Refers to the objID on an object defined elsewhere in the current document.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="objID" type="xsd:ID" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A unique identifier within this document for this object.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="E_RFP">
        <xsd:annotation>
            <xsd:documentation source="Description">Infomation pertaining to a request for proposal (meeting room and/or sleeping room information for a group).</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">E_RFP</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="rfp-0200:E_RFP_ID">
                <xsd:sequence>
                    <xsd:element minOccurs="1" ref="rfp-0200:RequestInfo">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifics about the Event.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="rfp-0200:Contacts">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Profile information for the submitter of the request.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="1" minOccurs="0" ref="rfp-0200:Dates">
                        <xsd:annotation>
                            <xsd:documentation source="Description">A collection of dates which allows you to request preferred and/or alternate dates.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="rfp-0200:Sites">
                        <xsd:annotation>
                            <xsd:documentation source="Description">A collection of sites for which the request is being submitted (e.g. a hotel, national sales office, convention center).</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="rfp-0200:RoomBlock">
                        <xsd:annotation>
                            <xsd:documentation source="Description">This is information pertaining to room requirements for this request.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="rfp-0200:EventBlockSubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">This is information pertaining to the event requirements for this request.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Summary"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="E_RFP_Detail">
        <xsd:annotation>
            <xsd:documentation source="Description">Infomation pertaining to a request for proposal (meeting room and/or sleeping room information for a group).</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">E_RFP</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="rfp-0200:E_RFP">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="AdditionalInfo" type="rfp-0200:AdditionalInfo">
                        <xsd:annotation>
                            <xsd:documentation source="Description">A collection of additional information requests.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Detail"/>
                </xsd:sequence>
                <xsd:attribute name="comments"
                    type="ota2-0200:StringTiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Comments related to the request.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="RoomBlock" type="rfp-0200:RoomBlock"/>
    
    <xsd:complexType name="RoomBlock">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the details for a room block.</xsd:documentation>
            <xsd:documentation source="Description">Specifies the details for a room block.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">RoomBlock</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="BudgetedRoomRate" type="ota2-0200:CurrencyAmount">
                <xsd:annotation>
                    <xsd:documentation source="Description">A general indication of the planner's budget for individual room nights and/or delegate rates.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="100" minOccurs="1" ref="rfp-0200:StayDay">
                <xsd:annotation>
                    <xsd:documentation source="Description">Room requirements for a particular day of the room block.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ReservationMethod" type="ota2-0200:ReservationMethod_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Rooming List, Individual, Housing Agency</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="organizerCheckInDaysBefore"
            type="xsd:integer" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Specifies the number of days the organizers will be checking in before the event.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = 2</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">2</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="organizerCheckOutDaysAfter"
            type="xsd:integer" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Specifies the number of days the organizers will be staying after the event.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = 1</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">1</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sleepingRoomInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, sleeping rooms are required.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="SpaceSize">
        <xsd:annotation>
            <xsd:documentation source="Description">The numeric value associated with the measurement.</xsd:documentation>
            <xsd:documentation source="Implementer">Example value: rfp1 = 1200</xsd:documentation>
            <xsd:documentation source="Implementer">Example value: rfp1 = 1200</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">SpaceSize</otm:OTA2Entity>
                <otm:Example context="rfp">1200</otm:Example>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:decimal">
                <xsd:attribute name="unitOfMeasure"
                    type="ota2-0200:StringTiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">This is the standard unit of measure name (e.g., it could be generic such as metric or imperial or specific such as inches, feet, yards, miles, millimeters, centimeters, meters, kilometers- according to usage).</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="rfp">Sq. Ft.</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:element name="Sites" type="rfp-0200:Sites"/>
    
    <xsd:complexType name="Sites">
        <xsd:annotation>
            <xsd:documentation source="Description">A collection of site information.</xsd:documentation>
            <xsd:documentation source="Description">A collection of site information.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Sites</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="100" minOccurs="0" ref="prod-0200:SiteSubGrp">
                <xsd:annotation>
                    <xsd:documentation source="Description">The site information for the RFP.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="StayDay" type="rfp-0200:StayDay"/>
    
    <xsd:complexType name="StayDay">
        <xsd:annotation>
            <xsd:documentation source="Description">Sleeping room details for a specific day of the event.</xsd:documentation>
            <xsd:documentation source="Description">Sleeping room details for a specific day of the event.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">StayDay</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="100" minOccurs="1" ref="rfp-0200:StayDayRoomSubGrp">
                <xsd:annotation>
                    <xsd:documentation source="Description">Identifies the room requirements.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="dayNumber" type="xsd:positiveInteger" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">The number which indicates the day of the room requirements (e.g. 1, 2, 3, etc).</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = 1</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">1</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element abstract="true" name="StayDayRoomSubGrp" type="rfp-0200:StayDayRoom">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the room requirements for this day of the room block.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">StayDayRoom</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="StayDayRoom"
        substitutionGroup="rfp-0200:StayDayRoomSubGrp" type="rfp-0200:StayDayRoom"/>
    <xsd:element name="StayDayRoomSummary" type="rfp-0200:StayDayRoom"/>
    <xsd:element name="StayDayRoomDetail"
        substitutionGroup="rfp-0200:StayDayRoomSubGrp" type="rfp-0200:StayDayRoom_Detail"/>
    
    <xsd:complexType name="StayDayRoom">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the room requirements for this day of the room block.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">StayDayRoom</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="prod-0200:RoomTypeSubGrp">
                <xsd:annotation>
                    <xsd:documentation source="Description">This is where a requester specifies the room type they wish to procure (e.g. king, double, double double, accessible). This is NOT an OpenTravel Codelist due to the infinite number of possible room types.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Summary"/>
        </xsd:sequence>
        <xsd:attribute name="numberOfUnits" type="xsd:integer" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">This is the required number of rooms for a particular day for a particular room type.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = 75</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">75</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="occupancyType" type="ota2-0200:StringShort" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Single, Double, Triple, Quad. Not ENUM.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = Single</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">Single</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="StayDayRoom_Detail">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the room requirements for this day of the room block.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">StayDayRoom</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="rfp-0200:StayDayRoom">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Detail"/>
                </xsd:sequence>
                <xsd:attribute name="comments"
                    type="ota2-0200:StringTiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Comment related to the StayDayRoom.</xsd:documentation>
                        <xsd:documentation source="Implementer">Example value: rfp1 = Comments about this stay day.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="rfp">Comments about this stay day.</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="Dates" type="rfp-0200:Dates"/>
    
    <xsd:complexType name="Dates">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the date(s) or date window(s) for the RFP.</xsd:documentation>
            <xsd:documentation source="Description">Specifies the date(s) or date window(s) for the RFP.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Dates</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="100" minOccurs="0" ref="ota2-0200:DateTimeSpanWindowsSubGrp">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the date range or date windows for the event.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="comments" type="ota2-0200:StringTiny" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Comments regarding the set of date ranges as a whole.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="flexibleInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, indicates that the event dates are flexible.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="AdditionalInfo">
        <xsd:annotation>
            <xsd:documentation source="Description">Group of indicators specifiying what additional information is provided.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">AdditionalInfo</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ota2:Empty">
                <xsd:attribute name="attachDocumentInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, there is a document attachment.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="aVNeedsInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, indicates that there are audio visual requirements for this event.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="decisionFactorsInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, indicates that there are specific decision factors for this event.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="contractualRequirementsInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, indicates that there are contractual requirements for this event.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="billingInfoInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, indicates that billing information is provided for this event.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="otherSuppliersInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, indicates that other suppliers are required for this event.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="thirdPartyPlannerInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, indicates that there is third party planner information included in the RFP.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:element name="CreateRQ"
        substitutionGroup="ota2msg:OTA2_Request_Payload" type="rfp-0200:Create_RQ"/>
    
