    <xsd:element name="EventSubFunctionDetail"
        substitutionGroup="rfp-0200:EventSubFunctionSubGrp" type="rfp-0200:EventSubFunction_Detail"/>
    
    <xsd:complexType name="EventSubFunction">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the details of the sub function.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">EventSubFunction</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Id" type="ota2-0200:Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">A unique ID for this event subfunction.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="SubFunctionType" type="ota2-0200:EventFunction_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Indicates the type of event being requested (e.g. breakfast meeting, reception).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Summary"/>
        </xsd:sequence>
        <xsd:attribute name="name" type="ota2-0200:StringShort" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Used to identify the event.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = Roll-in lunch</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">Roll-in lunch</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="startTime" type="xsd:time" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The time the event is scheduled to begin.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="endTime" type="xsd:time" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The time the event is scheduled to finish.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="attendeeQuantity"
            type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The number of people attending this event.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = 50</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">50</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="EventSubFunction_Detail">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the details of the sub function.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">EventSubFunction</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="rfp-0200:EventSubFunction">
                <xsd:sequence>
                    <xsd:element maxOccurs="100" minOccurs="0"
                        name="AVNeeds" type="ota2-0200:AudioVisual_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">AV requirments for this sub-function.</xsd:documentation>
