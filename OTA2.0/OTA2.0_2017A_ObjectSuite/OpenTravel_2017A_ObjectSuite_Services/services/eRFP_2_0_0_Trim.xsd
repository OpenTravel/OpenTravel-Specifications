            <xsd:element minOccurs="0" name="Identifier" type="ota2-0200:Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">A unique identifier defined by an external authority for this object.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="EventBlockRef" type="xsd:IDREF">
                <xsd:annotation>
                    <xsd:documentation source="Description">Refers to the objID on an object defined elsewhere in the current document.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="objID" type="xsd:ID" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A unique identifier within this document for this object.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="EventBlock">
        <xsd:annotation>
            <xsd:documentation source="Description">Information pertaining to the event requirements.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">EventBlock</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="rfp-0200:EventBlock_ID">
                <xsd:sequence>
                    <xsd:element maxOccurs="100" minOccurs="1" ref="rfp-0200:EventDay">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies event information for a single day.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0"
                        name="FoodAndBeveragesBudget" type="ota2-0200:CurrencyAmount">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies budget for food and beverage only.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
                </xsd:sequence>
                <xsd:attribute name="peakMeetingRoom" type="xsd:integer" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Number of meeting rooms needed on the peak day of the event.</xsd:documentation>
                        <xsd:documentation source="Implementer">Example value: rfp1 = 3</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="rfp">3</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="EventDay" type="rfp-0200:EventDay"/>
    
    <xsd:complexType name="EventDay">
        <xsd:annotation>
            <xsd:documentation source="Description">Function details for a specific day of the event.</xsd:documentation>
            <xsd:documentation source="Description">Function details for a specific day of the event.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">EventDay</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="100" minOccurs="0" ref="rfp-0200:EventFunctionSubGrp">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the details of the function.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="EventStatus" type="ota2-0200:EventStatus_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">The status for events for this day.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="dayNumber" type="xsd:positiveInteger" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">The number which indicates the day of the room requirements (e.g. 1, 2, 3, etc).</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = 1</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">1</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element abstract="true" name="EventFunctionSubGrp" type="rfp-0200:EventFunction">
