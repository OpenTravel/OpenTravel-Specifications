    <xsd:element abstract="true" name="CourseSubGrp" type="orggf-0100:Course_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Details of a golf course.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Course</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CourseID"
        substitutionGroup="orggf-0100:CourseSubGrp" type="orggf-0100:Course_ID"/>
    <xsd:element name="CourseIdentifier" type="orggf-0100:Course_ID"/>
    <xsd:element name="Course"
        substitutionGroup="orggf-0100:CourseSubGrp" type="orggf-0100:Course"/>
    <xsd:element name="CourseSummary" type="orggf-0100:Course"/>
    <xsd:element name="CourseDetail"
        substitutionGroup="orggf-0100:CourseSubGrp" type="orggf-0100:Course_Detail"/>
    
    <xsd:complexType name="Course_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Details of a golf course.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Course</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Identifier" type="ota2-0200:Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">A unique identifier defined by an external authority for this object.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="CourseRef" type="xsd:IDREF">
                <xsd:annotation>
                    <xsd:documentation source="Description">Refers to the obj ID on an object defined elsewhere in the current document.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="objID" type="xsd:ID" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A unique identifier within this document for this object.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="Course">
        <xsd:annotation>
            <xsd:documentation source="Description">Details of a golf course.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Course</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="orggf-0100:Course_ID">
                <xsd:sequence>
                    <xsd:element maxOccurs="20" minOccurs="0" ref="orggf-0100:CourseConditions">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Information about course conditions with an optional source name or URL.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="20" minOccurs="0" ref="orggf-0100:CourseRestrictionsSubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">A date and time period when restrictions apply to golf facility tee times.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="50" minOccurs="0" ref="orggf-0100:ScoreCard">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Scorecard information for the golf facility.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="30" minOccurs="0" ref="ota2-0200:OperationSchedule">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Hours of operation for the golf course.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="FeeStructure" type="ota2-0200:RateRange">
                        <xsd:annotation>
                            <xsd:documentation source="Description">General green fee amounts.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="CourseArchitect" type="orggf-0100:CourseArchitect">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Name of the designer of the golf course.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Summary"/>
                </xsd:sequence>
                <xsd:attribute name="numberOfHoles"
                    type="ota2-0200:NonNegativeInteger" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The number of holes offered at the golf course.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="par"
                    type="ota2-0200:NonNegativeInteger" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Par for the golf course.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="totalYardage"
                    type="ota2-0200:NonNegativeInteger" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Total yardage for the golf course.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="yearBuilt" type="xsd:date" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The year the course was built.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="name" type="ota2-0200:StringShort" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The name of the golf course.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="golfCarRequiredInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, a golf car is required on this golf course.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="Course_Detail">
        <xsd:annotation>
            <xsd:documentation source="Description">Details of a golf course.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Course</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="orggf-0100:Course">
                <xsd:sequence>
                    <xsd:element maxOccurs="10" minOccurs="0"
                        name="NineHoleName" type="ota2-0200:StringShort">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The name of a specific nine holes that make up part of the course.  This applies when a course has 18 or more holes where a user could request the back nine, or the name of the nine holes if the facility names them</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Detail"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="CourseConditions" type="orggf-0100:CourseConditions"/>
    
    <xsd:complexType name="CourseConditions">
        <xsd:annotation>
            <xsd:documentation source="Description">Information about course conditions with an optional source name or URL.</xsd:documentation>
            <xsd:documentation source="Description">Information about course conditions with an optional source name or URL.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">CourseConditions</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="source" type="ota2-0200:StringShort" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The source  that provided the course conditions.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="anyURI" type="xsd:anyURI" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The URL that provided the course conditions.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element abstract="true" name="CourseRestrictionsSubGrp" type="orggf-0100:CourseRestrictions">
        <xsd:annotation>
            <xsd:documentation source="Description">Restrictions that apply to this golf course.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">CourseRestrictions</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CourseRestrictions"
        substitutionGroup="orggf-0100:CourseRestrictionsSubGrp" type="orggf-0100:CourseRestrictions"/>
    <xsd:element name="CourseRestrictionsSummary" type="orggf-0100:CourseRestrictions"/>
    <xsd:element name="CourseRestrictionsDetail"
        substitutionGroup="orggf-0100:CourseRestrictionsSubGrp" type="orggf-0100:CourseRestrictions_Detail"/>
    
    <xsd:complexType name="CourseRestrictions">
        <xsd:annotation>
            <xsd:documentation source="Description">Restrictions that apply to this golf course.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">CourseRestrictions</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="TimeSpan" type="ota2-0200:TimeSpan">
                <xsd:annotation>
                    <xsd:documentation source="Description">Start and end dates.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Summary"/>
        </xsd:sequence>
        <xsd:attribute name="type" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The type of facility and/or tee time restriction.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="description" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A description of the facility and/or tee time restriction.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="CourseRestrictions_Detail">
        <xsd:annotation>
            <xsd:documentation source="Description">Restrictions that apply to this golf course.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">CourseRestrictions</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="orggf-0100:CourseRestrictions">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="StartWindow" type="ota2-0200:TimeSpan">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The earliest and latest dates acceptable for the start date.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="EndWindow" type="ota2-0200:TimeSpan">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The earliest and latest dates acceptable for the end date.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="100" minOccurs="0"
                        name="Duration" type="xsd:duration">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The duration data type represents a combination of year, month, day and time values representing a single duration of time, encoded as a single string.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="DurationUnit" type="ota2-0200:DurationUnit_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The unit of elapsed time or the day of the week applied to the value.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Detail"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="DrivingRange" type="orggf-0100:DrivingRange"/>
    
    <xsd:complexType name="DrivingRange">
        <xsd:annotation>
            <xsd:documentation source="Description">Driving range details for the golf facility.</xsd:documentation>
            <xsd:documentation source="Description">Driving range details for the golf facility.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">DrivingRange</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="turfType" type="ota2-0200:StringTiny" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The type of turf on the driving range.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="orggf">Grass</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="distance" type="ota2-0200:StringTiny" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Driving range distance.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="orggf">500 ft</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="numberOfStations"
            type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The number of stations at the driving range.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="orggf">10</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="drivingRangeType"
            type="orggf-0100:DrivingRangeType_Enum" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Describes the type of driving range (e.g. Covered, Indoor, Outdoor).</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="orggf">Outdoor</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="nightLightingInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, night lighting is provided.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="GolfFacilityFeatures" type="orggf-0100:GolfFacilityFeatures"/>
    
    <xsd:complexType name="GolfFacilityFeatures">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">GolfFacilityFeatures</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="10" minOccurs="0"
                name="FacilityHighlights" type="orggf-0100:FacilityHighlights_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Highlighted features of the golf facility.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="description" type="ota2-0200:String" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A description of the highlights of the facility.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="PracticeFacilities" type="orggf-0100:PracticeFacilities"/>
    
    <xsd:complexType name="PracticeFacilities">
        <xsd:annotation>
            <xsd:documentation source="Description">Details on a the practice facilities of a golf facility.</xsd:documentation>
            <xsd:documentation source="Description">Details on a the practice facilities of a golf facility.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">PracticeFacilities</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="30" minOccurs="0" ref="ota2-0200:OperationSchedule">
                <xsd:annotation>
                    <xsd:documentation source="Description">Operation schedules for the practice facilities.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="10" minOccurs="0" ref="orggf-0100:DrivingRange">
                <xsd:annotation>
                    <xsd:documentation source="Description">Details of the driving range.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="FeeStructure" type="ota2-0200:RateRange">
                <xsd:annotation>
                    <xsd:documentation source="Description">Fees associated with the practice facilities.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="20" minOccurs="0"
                name="PracticeFacilityFeatures" type="orggf-0100:PracticeFacilityFeatures_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Features of the practice facility.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element abstract="true" name="ProShopSubGrp" type="orggf-0100:ProShop_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Details of a golf facility pro shop.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">ProShop</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ProShopID"
        substitutionGroup="orggf-0100:ProShopSubGrp" type="orggf-0100:ProShop_ID"/>
    <xsd:element name="ProShopIdentifier" type="orggf-0100:ProShop_ID"/>
    <xsd:element name="ProShop"
        substitutionGroup="orggf-0100:ProShopSubGrp" type="orggf-0100:ProShop"/>
    <xsd:element name="ProShopSummary" type="orggf-0100:ProShop"/>
    
    <xsd:complexType name="ProShop_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Details of a golf facility pro shop.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">ProShop</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Identifier" type="ota2-0200:Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">A unique identifier defined by an external authority for this object.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ProShopRef" type="xsd:IDREF">
                <xsd:annotation>
                    <xsd:documentation source="Description">Refers to the obj ID on an object defined elsewhere in the current document.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="objID" type="xsd:ID" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A unique identifier within this document for this object.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="ProShop">
        <xsd:annotation>
            <xsd:documentation source="Description">Details of a golf facility pro shop.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">ProShop</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="orggf-0100:ProShop_ID">
                <xsd:sequence>
                    <xsd:element maxOccurs="30" minOccurs="0" ref="orggf-0100:RentalEquipment">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Club rental details.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="30" minOccurs="0"
                        name="Services" type="orggf-0100:GolfServices_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Services offered by the facility.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="GolfFacilitySearchCriteria" type="orggf-0100:GolfFacilitySearchCriteria"/>
    
    <xsd:complexType name="GolfFacilitySearchCriteria">
        <xsd:annotation>
            <xsd:documentation source="Description">Golf facility and tee time search criteria.</xsd:documentation>
            <xsd:documentation source="Description">Golf facility and tee time search criteria.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">GolfFacilitySearchCriteria</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="99" minOccurs="0"
                name="SpecifiedAmenity" type="ota2-0200:ListOfTinyStrings">
                <xsd:annotation>
                    <xsd:documentation source="Description">Amentities and services available with a product(either included in the price of the tee time or available for an extra fee), such as power carts. Note, includes pricing.</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}GolfAmenitySummaryType</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" ref="orggf-0100:GolfFacilitySubGrp">
                <xsd:annotation>
                    <xsd:documentation source="Description">Facility course description and features information.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="RateRange" type="ota2-0200:RateRange">
                <xsd:annotation>
                    <xsd:documentation source="Description">A min or max rate for green fees for this search.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2-0200:Location">
                <xsd:annotation>
                    <xsd:documentation source="Description">Defines the location of the search.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Radius" type="ota2-0200:Radius">
                <xsd:annotation>
                    <xsd:documentation source="Description">The radial distance for which a the search should be performed.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="golfFacilityName"
            type="ota2-0200:StringShort" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The name of the golf facility for which the search is being performed.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="orggf">Brookside Golf</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="ClubHouse" type="orggf-0100:ClubHouse"/>
    
    <xsd:complexType name="ClubHouse">
        <xsd:annotation>
            <xsd:documentation source="Description">Details of a golf facility club house.</xsd:documentation>
            <xsd:documentation source="Description">Details of a golf facility club house.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">ClubHouse</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="20" minOccurs="0"
                name="ClubHouseFeatures" type="orggf-0100:ClubHouseFeatures_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Features of the Club House.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:simpleType name="CourseType_Enum">
        <xsd:annotation>
            <xsd:documentation source="Description">The course type at the facility.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="EnumerationClosed">CourseType_Enum</otm:OTA2Entity>
            </xsd:appinfo>
