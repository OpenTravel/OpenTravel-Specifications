        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ota2-0200:TripPurpose_Enum">
                <xsd:attribute name="promptRequiredInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, the customer does not have a default trip purpose selected and they need to be prompted for the trip purpose.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
