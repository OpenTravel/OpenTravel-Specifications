            </xsd:element>
            <xsd:element minOccurs="0" ref="order-0100:Taxes">
                <xsd:annotation>
                    <xsd:documentation source="Description">Taxes for this product.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="99" minOccurs="0" ref="order-0100:Fee">
                <xsd:annotation>
                    <xsd:documentation source="Description">Fees related to this product.</xsd:documentation>
