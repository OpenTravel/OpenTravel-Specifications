                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}Comments</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">Comments</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="ShareAll" type="ota2-0200:Privacy">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Used to indicate whether information can be shared.</xsd:documentation>
                            <xsd:documentation source="Implementer">AttributeGroup: ShareAllGroup</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Type" type="ota2-0200:ProfileType_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Used to specify a profile type.</xsd:documentation>
                            <xsd:documentation source="Implementer">AttributeGroup: ProfileTypeGroup</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Status" type="ota2-0200:ProfileStatus_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Status of the profile. Refer to OpenTravel Code List Profile Status Code (PST).</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}ListOfOTA_CodeType</xsd:documentation>
