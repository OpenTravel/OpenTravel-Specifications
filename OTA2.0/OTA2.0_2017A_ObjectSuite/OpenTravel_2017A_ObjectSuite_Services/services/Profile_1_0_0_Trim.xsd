                    </xsd:element>
                    <xsd:element minOccurs="0" name="DateTimeStamp" type="ota2-0200:DateTimeStamp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Creation date time, creator ID, last modification date time and last modifier ID.</xsd:documentation>
                            <xsd:documentation source="Implementer">AttributeGroup: DateTimeStampGroup</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
