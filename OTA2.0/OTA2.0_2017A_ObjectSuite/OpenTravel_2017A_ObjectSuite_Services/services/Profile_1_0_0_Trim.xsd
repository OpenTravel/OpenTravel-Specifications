            <xsd:appinfo>
                <otm:OTA2Entity type="Operation">ProfileService_Update</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ota2msg:OTA2_Request_Payload">
                <xsd:sequence>
                    <xsd:element minOccurs="0"
                        name="PayloadStdAttributes" type="ota2-0200:PayloadStdAttributes">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The PayloadStdAttributes defines the standard attributes that appear on the root element for all OpenTravel Messages.</xsd:documentation>
                            <xsd:documentation source="Implementer">AttributeGroup: {http://www.opentravel.org/OTA/2003/05}OTA_PayloadStdAttributes</xsd:documentation>
