    <xsd:element name="ProfileID"
        substitutionGroup="prof-0100:ProfileSubGrp" type="prof-0100:Profile_ID"/>
    <xsd:element name="ProfileIdentifier" type="prof-0100:Profile_ID"/>
    <xsd:element name="Profile"
