    <xsd:element name="E_RFPID" substitutionGroup="rfp-0100:E_RFPSubGrp" type="rfp-0100:E_RFP_ID"/>
    <xsd:element name="E_RFPIdentifier"
        substitutionGroup="rfp-0100:E_RFPSubGrp" type="rfp-0100:E_RFP_ID"/>
    <xsd:element name="E_RFP" substitutionGroup="rfp-0100:E_RFPID" type="rfp-0100:E_RFP"/>
    <xsd:element name="E_RFPSummary"
        substitutionGroup="rfp-0100:E_RFPSubGrp" type="rfp-0100:E_RFP"/>
