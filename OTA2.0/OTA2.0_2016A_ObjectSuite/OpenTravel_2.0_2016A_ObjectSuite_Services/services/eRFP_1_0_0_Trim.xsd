    <xsd:element name="Contact" substitutionGroup="rfp-0100:ContactID" type="rfp-0100:Contact"/>
    <xsd:element name="ContactSummary"
        substitutionGroup="rfp-0100:ContactSubGrp" type="rfp-0100:Contact"/>
    <xsd:element name="ContactCompany"
        substitutionGroup="rfp-0100:Contact" type="rfp-0100:Contact_Company"/>
    <xsd:element name="ContactPerson"
        substitutionGroup="rfp-0100:Contact" type="rfp-0100:Contact_Person"/>
    
    <xsd:complexType name="Contact_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Contact for the event.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Contact</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Identifier" type="ota21:Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">Identifier provides the ability to create a globally unique identifier. For the identifier to be globally unique it must have a system provided identifier and the system must be identified using a global naming authority. System identification uses the domain naming system (DNS) to assure they are globally unique and should be an URL. The system provided ID will typically be a primary or surrogate key in a database.

The URL, system and company attributes can be omitted only when the system context can be implied by a parent or ancestor element.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="Contact">
        <xsd:annotation>
            <xsd:documentation source="Description">Contact for the event.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Contact</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="rfp-0100:Contact_ID">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
                </xsd:sequence>
                <xsd:attribute name="comments" type="ota2:String_Long" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Comments related to the contact.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="primaryInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Indicates that his is the primary contact.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="Contact_Company">
        <xsd:annotation>
            <xsd:documentation source="Description">Contact for the event.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Contact</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="rfp-0100:Contact">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="ota21:CompanySubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Detailed information about a company.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Custom"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="Contact_Person">
        <xsd:annotation>
            <xsd:documentation source="Description">Contact for the event.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Contact</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="rfp-0100:Contact">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="ota21:PersonSubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Detailed information about a contact person.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Custom"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="Contacts" type="rfp-0100:Contacts"/>
    
    <xsd:simpleType name="Contacts_Simple">
        <xsd:annotation>
            <xsd:documentation source="Description">An empty string used when the simple content of an element or attribute is to be omitted.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Contacts</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="ota2:Empty"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="Contacts_Simple_List">
        <xsd:list itemType="rfp-0100:Contacts_Simple"/>
    </xsd:simpleType>
    
    <xsd:complexType name="Contacts">
        <xsd:annotation>
            <xsd:documentation source="Description">A collection of contact information for each contact.</xsd:documentation>
            <xsd:documentation source="Description">A collection of contact information for each contact.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Contacts</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="100" minOccurs="1" ref="rfp-0100:ContactSubGrp">
                <xsd:annotation>
                    <xsd:documentation source="Description">Information related to a single contact.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="EventBlockSubGrp" type="rfp-0100:EventBlock_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Information pertaining to the event requirements.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">EventBlock</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="EventBlockID"
        substitutionGroup="rfp-0100:EventBlockSubGrp" type="rfp-0100:EventBlock_ID"/>
    <xsd:element name="EventBlockIdentifier"
        substitutionGroup="rfp-0100:EventBlockSubGrp" type="rfp-0100:EventBlock_ID"/>
    <xsd:element name="EventBlock"
        substitutionGroup="rfp-0100:EventBlockID" type="rfp-0100:EventBlock"/>
    <xsd:element name="EventBlockSummary"
        substitutionGroup="rfp-0100:EventBlockSubGrp" type="rfp-0100:EventBlock"/>
    
    <xsd:complexType name="EventBlock_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Information pertaining to the event requirements.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">EventBlock</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Identifier" type="ota21:Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">Identifier provides the ability to create a globally unique identifier. For the identifier to be globally unique it must have a system provided identifier and the system must be identified using a global naming authority. System identification uses the domain naming system (DNS) to assure they are globally unique and should be an URL. The system provided ID will typically be a primary or surrogate key in a database.

The URL, system and company attributes can be omitted only when the system context can be implied by a parent or ancestor element.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="EventBlock">
        <xsd:annotation>
            <xsd:documentation source="Description">Information pertaining to the event requirements.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">EventBlock</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="rfp-0100:EventBlock_ID">
                <xsd:sequence>
                    <xsd:element maxOccurs="100" minOccurs="1" ref="rfp-0100:EventDay">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies event information for a single day.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0"
                        name="FoodAndBeveragesBudget" type="ota21:Currency_Amount">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies budget for food and beverage only.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint"/>
                </xsd:sequence>
                <xsd:attribute name="peakMeetingRoom" type="xsd:integer" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Number of meeting rooms needed on the peak day of the event.</xsd:documentation>
                        <xsd:documentation source="Implementer">Example value: rfp1 = 3</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="rfp">3</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="EventDay" type="rfp-0100:EventDay"/>
    
    <xsd:simpleType name="EventDay_Simple">
        <xsd:annotation>
            <xsd:documentation source="Description">An empty string used when the simple content of an element or attribute is to be omitted.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">EventDay</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="ota2:Empty"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="EventDay_Simple_List">
        <xsd:list itemType="rfp-0100:EventDay_Simple"/>
    </xsd:simpleType>
    
    <xsd:complexType name="EventDay">
        <xsd:annotation>
            <xsd:documentation source="Description">Function details for a specific day of the event.</xsd:documentation>
            <xsd:documentation source="Description">Function details for a specific day of the event.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">EventDay</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="100" minOccurs="0" ref="rfp-0100:EventFunctionSubGrp">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the details of the function.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="EventStatus" type="ota21:EventStatus_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">The status for events for this day.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="dayNumber" type="xsd:positiveInteger" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">The number which indicates the day of the room requirements (e.g. 1, 2, 3, etc).</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = 1</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">1</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="EventFunctionSubGrp" type="rfp-0100:EventFunction">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the details of the function.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">EventFunction</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="EventFunction"
        substitutionGroup="rfp-0100:EventFunctionSubGrp" type="rfp-0100:EventFunction"/>
    <xsd:element name="EventFunctionSummary"
        substitutionGroup="rfp-0100:EventFunctionSubGrp" type="rfp-0100:EventFunction"/>
    <xsd:element name="EventFunctionDetail"
        substitutionGroup="rfp-0100:EventFunction" type="rfp-0100:EventFunction_Detail"/>
    
    <xsd:simpleType name="EventFunction_Simple">
        <xsd:annotation>
            <xsd:documentation source="Description">An empty string used when the simple content of an element or attribute is to be omitted.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">EventFunction</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="ota2:Empty"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="EventFunction_Simple_List">
        <xsd:list itemType="rfp-0100:EventFunction_Simple"/>
    </xsd:simpleType>
    
    <xsd:complexType name="EventFunction">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the details of the function.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">EventFunction</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="EventFunctionType" type="ota21:EventFunction_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Indicates the type of event being requested (e.g. breakfast meeting or reception).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="MeetingSpaceSize" type="rfp-0100:SpaceSize">
                <xsd:annotation>
                    <xsd:documentation source="Description">This identifies the space requirement.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
        <xsd:attribute name="eventId" type="xsd:ID" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A unique identifier for this event.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="startTime" type="xsd:time" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The time the event is scheduled to begin.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="endTime" type="xsd:time" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The time the event is scheduled to finish.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="attendeeQuantity"
            type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The number of people attending this event.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = 50</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">50</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="functionName" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The name of the event.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = Break-out session</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">Break-out session</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="meetingSpaceId" type="ota2:String_Tiny" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Identifies a specific meeting room.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = 246542</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">246542</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="meetingSpaceName" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The name identifying the meeting room or space.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = Grand Ballroom</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">Grand Ballroom</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="twentyFourHourHoldInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, indicates a room is held on a 24-hour basis (i.e., ensures a room set-up is not disturbed).</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="EventFunction_Detail">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the details of the function.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">EventFunction</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="rfp-0100:EventFunction">
                <xsd:sequence>
                    <xsd:element maxOccurs="100" minOccurs="0"
                        name="AVNeeds" type="ota21:AudioVisual_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">AV requirements for this function.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="RoomSetup" type="ota21:MeetingRoomFormat_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Indicates the arrangement of the meeting space (e.g. conference or banquet style). Refer to OpenTravel Code List Meeting Room Format Code (MRF).</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="100" minOccurs="0" ref="rfp-0100:EventSubFunctionSubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the details of a subfunction taking place within a function.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Detail"/>
                </xsd:sequence>
                <xsd:attribute name="comments" type="ota2:String_Text" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Comments regarding the event function.</xsd:documentation>
                        <xsd:documentation source="Implementer">Example value: rfp1 = Comments about this function.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="rfp">Comments about this function.</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="EventSubFunctionSubGrp" type="rfp-0100:EventSubFunction">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the details of the sub function.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">EventSubFunction</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="EventSubFunction"
        substitutionGroup="rfp-0100:EventSubFunctionSubGrp" type="rfp-0100:EventSubFunction"/>
    <xsd:element name="EventSubFunctionSummary"
        substitutionGroup="rfp-0100:EventSubFunctionSubGrp" type="rfp-0100:EventSubFunction"/>
    <xsd:element name="EventSubFunctionDetail"
        substitutionGroup="rfp-0100:EventSubFunction" type="rfp-0100:EventSubFunction_Detail"/>
    
    <xsd:simpleType name="EventSubFunction_Simple">
        <xsd:annotation>
            <xsd:documentation source="Description">An empty string used when the simple content of an element or attribute is to be omitted.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">EventSubFunction</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="ota2:Empty"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="EventSubFunction_Simple_List">
        <xsd:list itemType="rfp-0100:EventSubFunction_Simple"/>
    </xsd:simpleType>
    
    <xsd:complexType name="EventSubFunction">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the details of the sub function.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">EventSubFunction</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Id" type="ota21:Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">A unique ID for this event subfunction.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="SubFunctionType" type="ota21:EventFunction_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Indicates the type of event being requested (e.g. breakfast meeting, reception).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
        <xsd:attribute name="name" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Used to identify the event.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = Roll-in lunch</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">Roll-in lunch</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="startTime" type="xsd:time" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The time the event is scheduled to begin.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="endTime" type="xsd:time" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The time the event is scheduled to finish.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="attendeeQuantity"
            type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The number of people attending this event.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = 50</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">50</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="EventSubFunction_Detail">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the details of the sub function.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">EventSubFunction</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="rfp-0100:EventSubFunction">
                <xsd:sequence>
                    <xsd:element maxOccurs="100" minOccurs="0"
                        name="AVNeeds" type="ota21:AudioVisual_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">AV requirments for this sub-function.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Detail"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="RequestInfo" type="rfp-0100:RequestInfo"/>
    
    <xsd:simpleType name="RequestInfo_Simple">
        <xsd:annotation>
            <xsd:documentation source="Description">An empty string used when the simple content of an element or attribute is to be omitted.</xsd:documentation>
            <xsd:appinfo>
