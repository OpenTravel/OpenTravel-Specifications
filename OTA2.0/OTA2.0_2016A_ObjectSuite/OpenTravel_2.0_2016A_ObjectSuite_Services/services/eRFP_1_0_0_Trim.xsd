                <otm:OTA2Entity type="CoreObject">RequestInfo</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="EventType" type="ota21:EventType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">The type of event.</xsd:documentation>
                    <xsd:documentation source="Implementer">Example value: rfp1 = Conference</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="BusinessType" type="ota21:BusinessType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">The business category for this event.</xsd:documentation>
                    <xsd:documentation source="Implementer">Example value: rfp1 = Group</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="MarketSegment" type="ota21:InventoryBlockType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Used to identify the market segment for the meeting.</xsd:documentation>
                    <xsd:documentation source="Implementer">Example value: rfp1 = Corporate Group</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="TotalEventBudget" type="ota21:Currency_Amount">
                <xsd:annotation>
                    <xsd:documentation source="Description">A general indication of the planner's budget for the event.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="LargestMeetingSpace" type="rfp-0100:SpaceSize">
                <xsd:annotation>
                    <xsd:documentation source="Description">The largest meeting room required for the event.</xsd:documentation>
                    <xsd:documentation source="Implementer">Example value: rfp1 = 1000 sq ft</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="TotalMeetingSpace" type="rfp-0100:SpaceSize">
                <xsd:annotation>
                    <xsd:documentation source="Description">The total amount of meeting space needed for the event.</xsd:documentation>
                    <xsd:documentation source="Implementer">Example value: rfp1 = 7000 sq ft</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ExpectedCommissionRate" type="ota21:AmountPercent">
                <xsd:annotation>
                    <xsd:documentation source="Description">The commission rate expected for this event.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PreferredContactMethod" type="ota21:DistributionType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">The prefered method of contact.</xsd:documentation>
                    <xsd:documentation source="Implementer">Example value: rfp1 = Email</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:ID" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Unique identifier supplied by the requesting system.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="rFPName" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A name identifiying the RFP.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = Booking for ABC Convention</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">ABC Convention</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="responseDue"
            type="ota2:DateTime_WithTimeZone" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The date the requestor requires a response/proposal.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="decisionDue"
            type="ota2:DateTime_WithTimeZone" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The date in which the client intends to make a decision.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="totalAttendees" type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">An estimate of the total number of attendees for the event.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = 200</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">500</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="peakRoomQuantity"
            type="xsd:positiveInteger" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The number of rooms needed for the peak night of the event.</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = 100</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">100</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="rFPDescription" type="ota2:String_Text" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A comment field used to convey free form information about the RFP</xsd:documentation>
                <xsd:documentation source="Implementer">Example value: rfp1 = Text about the event.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="rfp">Text about the event.</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="eventName" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A name identifiying the event.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="roomBlockInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, indicates a room block is requested. When false, indicates no room block is requested and RoomBlock will not be passed.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="eventBlockInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, indicates function space is required. When false, indicates no function space is required and EventBlock will not be passed.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="commisionInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, commission will be paid on this event.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="E_RFPSubGrp" type="rfp-0100:E_RFP_ID">
