    <xsd:element name="ContactSubGrp" type="rfp-0100:Contact_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Contact for the event.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Contact</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ContactID"
        substitutionGroup="rfp-0100:ContactSubGrp" type="rfp-0100:Contact_ID"/>
    <xsd:element name="ContactIdentifier"
