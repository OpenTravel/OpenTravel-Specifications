    <xsd:complexType name="Segment_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Segment</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" ref="prod-0100:DeparturePointSubGrp"/>
            <xsd:element minOccurs="1" ref="prod-0100:TransportNumberSubGrp"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:ID" use="optional"/>
        <xsd:attribute name="departureDateTime"
            type="ota2:DateTime_Local" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">ISO date time type without UTC offset or Z for Zulu restriction indicating it is representing Local Time
Example: 2010-12-31T11:55:00</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type:
				{http://www.w3.org/2001/XMLSchema}dateTime</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="Segment">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Segment</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prod-0100:Segment_ID">
                <xsd:sequence>
                    <xsd:element minOccurs="1" ref="prod-0100:ArrivalPointSubGrp"/>
                    <xsd:element maxOccurs="26" minOccurs="1" ref="prod-0100:ClassOfServiceSubGrp"/>
                    <xsd:element maxOccurs="25" minOccurs="0" ref="prod-0100:AncillaryProductSubGrp"/>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
                </xsd:sequence>
                <xsd:attribute name="arrivalDateTime"
                    type="ota2:DateTime_Local" use="required">
                    <xsd:annotation>
                        <xsd:documentation source="Description">ISO date time type without UTC offset or Z for Zulu restriction indicating it is representing Local Time
Example: 2010-12-31T11:55:00</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type:
				{http://www.w3.org/2001/XMLSchema}dateTime</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="numberOfStops"
                    type="xsd:positiveInteger" use="required"/>
                <xsd:attribute name="duration" type="xsd:duration" use="required"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
