    <xsd:element name="AncillaryProductSubGrp" type="prod-0100:AncillaryProduct_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">AncillaryProduct</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="AncillaryProductID"
        substitutionGroup="prod-0100:AncillaryProductSubGrp" type="prod-0100:AncillaryProduct_ID"/>
    <xsd:element name="AncillaryProductIdentifier"
        substitutionGroup="prod-0100:AncillaryProductSubGrp" type="prod-0100:AncillaryProduct_ID"/>
    <xsd:element name="AncillaryProduct"
        substitutionGroup="prod-0100:AncillaryProductID" type="prod-0100:AncillaryProduct"/>
    <xsd:element name="AncillaryProductSummary"
        substitutionGroup="prod-0100:AncillaryProductSubGrp" type="prod-0100:AncillaryProduct"/>
    <xsd:element name="AncillaryProductDetail"
        substitutionGroup="prod-0100:AncillaryProduct" type="prod-0100:AncillaryProduct_Detail"/>
    
    <xsd:complexType name="AncillaryProduct_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">AncillaryProduct</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:attribute name="id" type="xsd:ID" use="optional"/>
        <xsd:attribute name="code" type="ota2:String_Tiny" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">A tiny string  
Minimum length of 0 and a maximum length of 32</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="AncillaryProduct">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">AncillaryProduct</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prod-0100:AncillaryProduct_ID">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
                </xsd:sequence>
                <xsd:attribute name="name" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A tiny string  
Minimum length of 0 and a maximum length of 32</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="AncillaryProduct_Detail">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">AncillaryProduct</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prod-0100:AncillaryProduct">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Detail"/>
                </xsd:sequence>
                <xsd:attribute name="description"
                    type="ota2:String_Long" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A long string  
Minimum length of 0 and a maximum length of 4096</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="BasicPropertyInfoSubGrp" type="prod-0100:BasicPropertyInfo">
        <xsd:annotation>
            <xsd:documentation source="Description">Identifies a property.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">BasicPropertyInfo</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="BasicPropertyInfo"
        substitutionGroup="prod-0100:BasicPropertyInfoSubGrp" type="prod-0100:BasicPropertyInfo"/>
    <xsd:element name="BasicPropertyInfoSummary"
        substitutionGroup="prod-0100:BasicPropertyInfoSubGrp" type="prod-0100:BasicPropertyInfo"/>
    <xsd:element name="BasicPropertyInfoDetail"
        substitutionGroup="prod-0100:BasicPropertyInfo" type="prod-0100:BasicPropertyInfo_Detail"/>
    
    <xsd:element name="SiteSubGrp" type="prod-0100:BasicPropertyInfo">
        <xsd:annotation>
            <xsd:documentation source="Description">Identifies a property.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">BasicPropertyInfo</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="Site" substitutionGroup="prod-0100:SiteSubGrp" type="prod-0100:BasicPropertyInfo"/>
    <xsd:element name="SiteSummary"
        substitutionGroup="prod-0100:SiteSubGrp" type="prod-0100:BasicPropertyInfo"/>
    <xsd:element name="SiteDetail" substitutionGroup="prod-0100:Site" type="prod-0100:BasicPropertyInfo_Detail"/>
    
    <xsd:simpleType name="BasicPropertyInfo_Simple">
        <xsd:annotation>
            <xsd:documentation source="Description">An empty string used when the simple content of an element or attribute is to be omitted.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">BasicPropertyInfo</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="ota2:Empty"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="BasicPropertyInfo_Simple_List">
        <xsd:list itemType="prod-0100:BasicPropertyInfo_Simple"/>
    </xsd:simpleType>
    
    <xsd:complexType name="BasicPropertyInfo">
        <xsd:annotation>
            <xsd:documentation source="Description">Identifies a property.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">BasicPropertyInfo</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
        </xsd:sequence>
        <xsd:attribute name="code" type="ota2:String_Tiny" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Uniquely identifies a single property for a reservation.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="product">CHIDT</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="BasicPropertyInfo_Detail">
        <xsd:annotation>
            <xsd:documentation source="Description">Identifies a property.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">BasicPropertyInfo</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prod-0100:BasicPropertyInfo">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Detail"/>
                </xsd:sequence>
                <xsd:attribute name="name" type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Specifies the proper name of the property.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="product">ABC Hotel</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="cityCode" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Specifies the three character IATA city code.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="product">CHI</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="chainCode" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Identifies the chain.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="product">AA</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="chainName" type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Specifies the proper name of the chain.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="product">XYZ Chain</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="brandCode" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Identifies a brand with in a chain (e.g. Courtyard, Hampton Inn).  This code is optional if the hotel is an independent property that can be identified by a propert code alone.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="product">BB</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="brandName" type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Identifies the proper name of the brand.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="product">BB Brand</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="ClassOfServiceSubGrp" type="prod-0100:ClassOfService_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">ClassOfService</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ClassOfServiceID"
        substitutionGroup="prod-0100:ClassOfServiceSubGrp" type="prod-0100:ClassOfService_ID"/>
    <xsd:element name="ClassOfServiceIdentifier"
        substitutionGroup="prod-0100:ClassOfServiceSubGrp" type="prod-0100:ClassOfService_ID"/>
    <xsd:element name="ClassOfService"
        substitutionGroup="prod-0100:ClassOfServiceID" type="prod-0100:ClassOfService"/>
    <xsd:element name="ClassOfServiceSummary"
        substitutionGroup="prod-0100:ClassOfServiceSubGrp" type="prod-0100:ClassOfService"/>
    <xsd:element name="ClassOfServiceDetail"
        substitutionGroup="prod-0100:ClassOfService" type="prod-0100:ClassOfService_Detail"/>
    
    <xsd:complexType name="ClassOfService_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">ClassOfService</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:attribute name="code" type="ota2:String_Tiny" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">Used to identify this product programmatically.  EG "F" for First Class</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="ClassOfService">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">ClassOfService</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prod-0100:ClassOfService_ID">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
                </xsd:sequence>
                <xsd:attribute name="count" type="xsd:positiveInteger" use="required">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Number available for sale or that have been sold</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="ClassOfService_Detail">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">ClassOfService</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prod-0100:ClassOfService">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Detail"/>
                </xsd:sequence>
                <xsd:attribute name="name" type="ota2:String_Short" use="required">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Display name for this product.  EG "First Class"</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="GuestCount">
        <xsd:annotation>
            <xsd:documentation source="Description">Identifies the number of guests for the reservation.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">GuestCount</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:positiveInteger">
                <xsd:attribute name="ageQualifyingCode"
                    type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Specifies the age type of the guest.  (e.g. Adult, Child, Infant).</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="age" type="xsd:positiveInteger" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Specifies the specific age of the guest.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:element name="JourneySubGrp" type="prod-0100:Journey_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Journey</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="JourneyID"
        substitutionGroup="prod-0100:JourneySubGrp" type="prod-0100:Journey_ID"/>
    <xsd:element name="JourneyIdentifier"
        substitutionGroup="prod-0100:JourneySubGrp" type="prod-0100:Journey_ID"/>
    <xsd:element name="Journey" substitutionGroup="prod-0100:JourneyID" type="prod-0100:Journey"/>
    <xsd:element name="JourneySummary"
        substitutionGroup="prod-0100:JourneySubGrp" type="prod-0100:Journey"/>
    <xsd:element name="JourneyQueryProduct" type="prod-0100:Journey_Query_Product"/>
    
    <xsd:complexType name="Journey_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Journey</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:attribute name="id" type="xsd:ID" use="optional"/>
    </xsd:complexType>
    
    <xsd:complexType name="Journey">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Journey</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prod-0100:Journey_ID">
                <xsd:sequence>
                    <xsd:element maxOccurs="6" minOccurs="1" ref="prod-0100:SegmentSubGrp"/>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint"/>
                </xsd:sequence>
                <xsd:attribute name="duration" type="xsd:duration" use="required"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="Journey_Query_Product">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Journey</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" ref="prod-0100:DeparturePointIdentifier"/>
            <xsd:element minOccurs="1" ref="prod-0100:ArrivalPointIdentifier"/>
            <xsd:element minOccurs="0" ref="prod-0100:TransportNumberSummary"/>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="departureDateTime" type="xsd:time" use="required"/>
    </xsd:complexType>
    
    <xsd:element name="LocationPointSubGrp" type="prod-0100:LocationPoint_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">LocationPoint</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="LocationPointID"
        substitutionGroup="prod-0100:LocationPointSubGrp" type="prod-0100:LocationPoint_ID"/>
    <xsd:element name="LocationPointIdentifier"
        substitutionGroup="prod-0100:LocationPointSubGrp" type="prod-0100:LocationPoint_ID"/>
    <xsd:element name="LocationPoint"
        substitutionGroup="prod-0100:LocationPointID" type="prod-0100:LocationPoint"/>
    <xsd:element name="LocationPointSummary"
        substitutionGroup="prod-0100:LocationPointSubGrp" type="prod-0100:LocationPoint"/>
    <xsd:element name="LocationPointDetail"
        substitutionGroup="prod-0100:LocationPoint" type="prod-0100:LocationPoint_Detail"/>
    
    <xsd:element name="ArrivalPointSubGrp" type="prod-0100:LocationPoint_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">LocationPoint</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ArrivalPointID"
        substitutionGroup="prod-0100:ArrivalPointSubGrp" type="prod-0100:LocationPoint_ID"/>
    <xsd:element name="ArrivalPointIdentifier"
        substitutionGroup="prod-0100:ArrivalPointSubGrp" type="prod-0100:LocationPoint_ID"/>
    <xsd:element name="ArrivalPoint"
        substitutionGroup="prod-0100:ArrivalPointID" type="prod-0100:LocationPoint"/>
    <xsd:element name="ArrivalPointSummary"
        substitutionGroup="prod-0100:ArrivalPointSubGrp" type="prod-0100:LocationPoint"/>
    <xsd:element name="ArrivalPointDetail"
        substitutionGroup="prod-0100:ArrivalPoint" type="prod-0100:LocationPoint_Detail"/>
    
    <xsd:element name="DeparturePointSubGrp" type="prod-0100:LocationPoint_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">LocationPoint</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="DeparturePointID"
        substitutionGroup="prod-0100:DeparturePointSubGrp" type="prod-0100:LocationPoint_ID"/>
    <xsd:element name="DeparturePointIdentifier"
        substitutionGroup="prod-0100:DeparturePointSubGrp" type="prod-0100:LocationPoint_ID"/>
    <xsd:element name="DeparturePoint"
        substitutionGroup="prod-0100:DeparturePointID" type="prod-0100:LocationPoint"/>
    <xsd:element name="DeparturePointSummary"
        substitutionGroup="prod-0100:DeparturePointSubGrp" type="prod-0100:LocationPoint"/>
    <xsd:element name="DeparturePointDetail"
        substitutionGroup="prod-0100:DeparturePoint" type="prod-0100:LocationPoint_Detail"/>
    
    <xsd:complexType name="LocationPoint_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">LocationPoint</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:attribute name="code" type="ota2:String_Tiny" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">A tiny string  
Minimum length of 0 and a maximum length of 32</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="LocationPoint">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">LocationPoint</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prod-0100:LocationPoint_ID">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Summary"/>
                </xsd:sequence>
                <xsd:attribute name="kioskInd" type="xsd:boolean" use="optional"/>
                <xsd:attribute name="curbSideBaggageCheckInd"
                    type="xsd:boolean" use="optional"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="LocationPoint_Detail">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">LocationPoint</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prod-0100:LocationPoint">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint_Detail"/>
                </xsd:sequence>
                <xsd:attribute name="openingTime" type="xsd:time" use="required"/>
                <xsd:attribute name="closingTime" type="xsd:time" use="required"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="Meal" type="prod-0100:Meal"/>
    
    <xsd:simpleType name="Meal_Simple">
        <xsd:annotation>
            <xsd:documentation source="Description">An empty string used when the simple content of an element or attribute is to be omitted.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Meal</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="ota2:Empty"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="Meal_Simple_List">
        <xsd:list itemType="prod-0100:Meal_Simple"/>
    </xsd:simpleType>
    
    <xsd:complexType name="Meal">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Meal</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" ref="prod-0100:ClassOfServiceSubGrp">
                <xsd:annotation>
                    <xsd:documentation source="Description">Product that includes these meals</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="3" minOccurs="1" name="MealCode" type="prod-0100:Enum_Meal">
                <xsd:annotation>
                    <xsd:documentation source="Description">Meal(s) served</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2msg:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="MealsIncluded">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies which meals are included with this rate plan.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">MealsIncluded</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ota2:Empty">
                <xsd:attribute name="mealPlanCodes"
                    type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Identifies the type of meal plans included with this rate plan.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="breakfastInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, indicates that breakfast is included with this rate plan.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="lunchInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, indicates that lunch is included with this rate plan.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="dinnerInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, indicates dinner is included with this rate plan.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:element name="ProductSubGrp" type="prod-0100:Product_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Product</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ProductID"
        substitutionGroup="prod-0100:ProductSubGrp" type="prod-0100:Product_ID"/>
